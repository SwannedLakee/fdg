{	
"sn2.6:0.1":	"संयुत्त निकाय २।६ ",
"sn2.6:0.2":	"१। पठमवग्ग ",
"sn2.6:0.3":	"कामदसुत्त ",
"sn2.6:1.1":	"सावत्थिनिदानं। ",
"sn2.6:1.2":	"एकमन्तं ठितो खो कामदो देवपुत्तो भगवन्तं एतदवोच: ",
"sn2.6:1.3":	"“दुक्करं, भगवा, सुदुक्करं, भगवा”ति। ",
"sn2.6:2.1":	"“दुक्करं वापि करोन्ति, ",
"sn2.6:2.2":	"(कामदाति भगवा) ",
"sn2.6:2.3":	"सेखा सीलसमाहिता; ",
"sn2.6:2.4":	"ठितत्ता अनगारियुपेतस्स, ",
"sn2.6:2.5":	"तुट्ठि होति सुखावहा”ति। ",
"sn2.6:3.1":	"“दुल्लभा, भगवा, यदिदं तुट्ठी”ति। ",
"sn2.6:4.1":	"“दुल्लभं वापि लभन्ति, ",
"sn2.6:4.2":	"(कामदाति भगवा) ",
"sn2.6:4.3":	"चित्तवूपसमे रता; ",
"sn2.6:4.4":	"येसं दिवा च रत्तो च, ",
"sn2.6:4.5":	"भावनाय रतो मनो”ति। ",
"sn2.6:5.1":	"“दुस्समादहं, भगवा, यदिदं चित्तन्”ति। ",
"sn2.6:6.1":	"“दुस्समादहं वापि समादहन्ति, ",
"sn2.6:6.2":	"(कामदाति भगवा) ",
"sn2.6:6.3":	"इन्द्रियूपसमे रता; ",
"sn2.6:6.4":	"ते छेत्वा मच्चुनो जालं, ",
"sn2.6:6.5":	"अरिया गच्छन्ति कामदा”ति। ",
"sn2.6:7.1":	"“दुग्गमो, भगवा, विसमो मग्गो”ति। ",
"sn2.6:8.1":	"“दुग्गमे विसमे वापि, ",
"sn2.6:8.2":	"अरिया गच्छन्ति कामद; ",
"sn2.6:8.3":	"अनरिया विसमे मग्गे, ",
"sn2.6:8.4":	"पपतन्ति अवंसिरा; ",
"sn2.6:8.5":	"अरियानं समो मग्गो, ",
"sn2.6:8.6":	"अरिया हि विसमे समा”ति। "
}	
