{	
"ja414:0.1":	"जातक ",
"ja414:0.2":	"सत्तकनिपात ",
"ja414:0.3":	"गन्धारवग्ग ",
"ja414:0.4":	"९। जागरजातक ",
"ja414:1.1":	"“कोध जागरतं सुत्तो, ",
"ja414:1.2":	"कोध सुत्तेसु जागरो; ",
"ja414:1.3":	"को ममेतं विजानाति, ",
"ja414:1.4":	"को तं पटिभणाति मे”। ",
"ja414:2.1":	"“अहं जागरतं सुत्तो, ",
"ja414:2.2":	"अहं सुत्तेसु जागरो; ",
"ja414:2.3":	"अहमेतं विजानामि, ",
"ja414:2.4":	"अहं पटिभणामि ते”। ",
"ja414:3.1":	"“कथं जागरतं सुत्तो, ",
"ja414:3.2":	"कथं सुत्तेसु जागरो; ",
"ja414:3.3":	"कथं एतं विजानासि, ",
"ja414:3.4":	"कथं पटिभणासि मे”। ",
"ja414:4.1":	"“ये धम्मं नप्पजानन्ति, ",
"ja414:4.2":	"संयमोति दमोति च; ",
"ja414:4.3":	"तेसु सुप्पमानेसु, ",
"ja414:4.4":	"अहं जग्गामि देवते। ",
"ja414:5.1":	"येसं रागो च दोसो च, ",
"ja414:5.2":	"अविज्जा च विराजिता; ",
"ja414:5.3":	"तेसु जागरमानेसु, ",
"ja414:5.4":	"अहं सुत्तोस्मि देवते। ",
"ja414:6.1":	"एवं जागरतं सुत्तो, ",
"ja414:6.2":	"एवं सुत्तेसु जागरो; ",
"ja414:6.3":	"एवमेतं विजानामि, ",
"ja414:6.4":	"एवं पटिभणामि ते”। ",
"ja414:7.1":	"“साधु जागरतं सुत्तो, ",
"ja414:7.2":	"साधु सुत्तेसु जागरो; ",
"ja414:7.3":	"साधुमेतं विजानासि, ",
"ja414:7.4":	"साधु पटिभणासि मे”ति। ",
"ja414:8.1":	"जागरजातकं नवमं। "
}	
