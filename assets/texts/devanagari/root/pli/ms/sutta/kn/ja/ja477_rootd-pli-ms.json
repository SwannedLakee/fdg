{	
"ja477:0.1":	"जातक ",
"ja477:0.2":	"तेरसकनिपात ",
"ja477:0.3":	"अम्बवग्ग ",
"ja477:0.4":	"४। चूळनारदजातक ",
"ja477:1.1":	"“न ते कट्ठानि भिन्नानि, ",
"ja477:1.2":	"न ते उदकमाभतं; ",
"ja477:1.3":	"अग्गीपि ते न हापितो, ",
"ja477:1.4":	"किं नु मन्दोव झायसि”। ",
"ja477:2.1":	"“न उस्सहे वने वत्थुं, ",
"ja477:2.2":	"कस्सपामन्तयामि तं; ",
"ja477:2.3":	"दुक्खो वासो अरञ्ञस्मिं, ",
"ja477:2.4":	"रट्ठं इच्छामि गन्तवे। ",
"ja477:3.1":	"यथा अहं इतो गन्त्वा, ",
"ja477:3.2":	"यस्मिं जनपदे वसं; ",
"ja477:3.3":	"आचारं ब्रह्मे सिक्खेय्यं, ",
"ja477:3.4":	"तं धम्मं अनुसास मं”। ",
"ja477:4.1":	"“सचे अरञ्ञं हित्वान, ",
"ja477:4.2":	"वनमूलफलानि च; ",
"ja477:4.3":	"रट्ठे रोचयसे वासं, ",
"ja477:4.4":	"तं धम्मं निसामेहि मे। ",
"ja477:5.1":	"विसं मा पटिसेवित्थो, ",
"ja477:5.2":	"पपातं परिवज्जय; ",
"ja477:5.3":	"पङ्के च मा विसीदित्थो, ",
"ja477:5.4":	"यत्तो चासीविसे चरे”। ",
"ja477:6.1":	"“किं नु विसं पपातो वा, ",
"ja477:6.2":	"पङ्को वा ब्रह्मचारिनं; ",
"ja477:6.3":	"कं त्वं आसीविसं ब्रूसि, ",
"ja477:6.4":	"तं मे अक्खाहि पुच्छितो”। ",
"ja477:7.1":	"“आसवो तात लोकस्मिं, ",
"ja477:7.2":	"सुरा नाम पवुच्चति; ",
"ja477:7.3":	"मनुञ्ञो सुरभी वग्गु, ",
"ja477:7.4":	"सादु खुद्दरसूपमो; ",
"ja477:7.5":	"विसं तदाहु अरिया से, ",
"ja477:7.6":	"ब्रह्मचरियस्स नारद। ",
"ja477:8.1":	"इत्थियो तात लोकस्मिं, ",
"ja477:8.2":	"पमत्तं पमथेन्ति ता; ",
"ja477:8.3":	"हरन्ति युविनो चित्तं, ",
"ja477:8.4":	"तूलं भट्ठंव मालुतो; ",
"ja477:8.5":	"पपातो एसो अक्खातो, ",
"ja477:8.6":	"ब्रह्मचरियस्स नारद। ",
"ja477:9.1":	"लाभो सिलोको सक्कारो, ",
"ja477:9.2":	"पूजा परकुलेसु च; ",
"ja477:9.3":	"पङ्को एसो च अक्खातो, ",
"ja477:9.4":	"ब्रह्मचरियस्स नारद। ",
"ja477:10.1":	"ससत्था तात राजानो, ",
"ja477:10.2":	"आवसन्ति महिं इमं; ",
"ja477:10.3":	"ते तादिसे मनुस्सिन्दे, ",
"ja477:10.4":	"महन्ते तात नारद। ",
"ja477:11.1":	"इस्सरानं अधिपतीनं, ",
"ja477:11.2":	"न तेसं पादतो चरे; ",
"ja477:11.3":	"आसीविसोति अक्खातो, ",
"ja477:11.4":	"ब्रह्मचरियस्स नारद। ",
"ja477:12.1":	"भत्तत्थो भत्तकाले च, ",
"ja477:12.2":	"यं गेहमुपसङ्कमे; ",
"ja477:12.3":	"यदेत्थ कुसलं जञ्ञा, ",
"ja477:12.4":	"तत्थ घासेसनं चरे। ",
"ja477:13.1":	"पविसित्वा परकुलं, ",
"ja477:13.2":	"पानत्थं भोजनाय वा; ",
"ja477:13.3":	"मितं खादे मितं भुञ्जे, ",
"ja477:13.4":	"न च रूपे मनं करे। ",
"ja477:14.1":	"गोट्ठं मज्जं किराटञ्च, ",
"ja477:14.2":	"सभा निकिरणानि च; ",
"ja477:14.3":	"आरका परिवज्जेहि, ",
"ja477:14.4":	"यानीव विसमं पथन्”ति। ",
"ja477:15.1":	"चूळनारदजातकं चतुत्थं। "
}	
