{	
"ja476:0.1":	"जातक ",
"ja476:0.2":	"तेरसकनिपात ",
"ja476:0.3":	"अम्बवग्ग ",
"ja476:0.4":	"३। जवनहंसजातक ",
"ja476:1.1":	"“इधेव हंस निपत, ",
"ja476:1.2":	"पियं मे तव दस्सनं; ",
"ja476:1.3":	"इस्सरोसि अनुप्पत्तो, ",
"ja476:1.4":	"यमिधत्थि पवेदय”। ",
"ja476:2.1":	"“सवनेन एकस्स पिया भवन्ति, ",
"ja476:2.2":	"दिस्वा पनेकस्स वियेति छन्दो; ",
"ja476:2.3":	"दिस्वा च सुत्वा च पिया भवन्ति, ",
"ja476:2.4":	"कच्चिन्नु मे पीयसि दस्सनेन। ",
"ja476:3.1":	"सवनेन पियो मेसि, ",
"ja476:3.2":	"भिय्यो चागम्म दस्सनं; ",
"ja476:3.3":	"एवं पियदस्सनो मे, ",
"ja476:3.4":	"वस हंस ममन्तिके”। ",
"ja476:4.1":	"“वसेय्याम तवागारे, ",
"ja476:4.2":	"निच्चं सक्कतपूजिता; ",
"ja476:4.3":	"मत्तो च एकदा वज्जे, ",
"ja476:4.4":	"‘हंसराजं पचन्तु मे’”। ",
"ja476:5.1":	"“धिरत्थु तं मज्जपानं, ",
"ja476:5.2":	"यं मे पियतरं तया; ",
"ja476:5.3":	"न चापि मज्जं पिस्सामि, ",
"ja476:5.4":	"याव मे वच्छसी घरे”। ",
"ja476:6.1":	"“सुविजानं सिङ्गालानं, ",
"ja476:6.2":	"सकुणानञ्च वस्सितं; ",
"ja476:6.3":	"मनुस्सवस्सितं राज, ",
"ja476:6.4":	"दुब्बिजानतरं ततो। ",
"ja476:7.1":	"अपि चे मञ्ञती पोसो, ",
"ja476:7.2":	"ञाति मित्तो सखाति वा; ",
"ja476:7.3":	"यो पुब्बे सुमनो हुत्वा, ",
"ja476:7.4":	"पच्छा सम्पज्जते दिसो। ",
"ja476:8.1":	"यस्मिं मनो निविसति, ",
"ja476:8.2":	"अविदूरे सहापि सो; ",
"ja476:8.3":	"सन्तिकेपि हि सो दूरे, ",
"ja476:8.4":	"यस्मिं नाविसते मनो। ",
"ja476:9.1":	"अन्तोपि सो होति पसन्नचित्तो, ",
"ja476:9.2":	"पारं समुद्दस्स पसन्नचित्तो; ",
"ja476:9.3":	"अन्तोपि सो होति पदुट्ठचित्तो, ",
"ja476:9.4":	"पारं समुद्दस्स पदुट्ठचित्तो। ",
"ja476:10.1":	"संवसन्ता विवसन्ति, ",
"ja476:10.2":	"ये दिसा ते रथेसभ; ",
"ja476:10.3":	"आरा सन्तो संवसन्ति, ",
"ja476:10.4":	"मनसा रट्ठवड्ढन। ",
"ja476:11.1":	"अतिचिरं निवासेन, ",
"ja476:11.2":	"पियो भवति अप्पियो; ",
"ja476:11.3":	"आमन्त खो तं गच्छाम, ",
"ja476:11.4":	"पुरा ते होम अप्पिया”। ",
"ja476:12.1":	"“एवञ्चे याचमानानं, ",
"ja476:12.2":	"अञ्जलिं नावबुज्झसि; ",
"ja476:12.3":	"परिचारकानं सतं, ",
"ja476:12.4":	"वचनं न करोसि नो; ",
"ja476:12.5":	"एवं तं अभियाचाम, ",
"ja476:12.6":	"पुन कयिरासि परियायं”। ",
"ja476:13.1":	"“एवञ्चे नो विहरतं, ",
"ja476:13.2":	"अन्तरायो न हेस्सति; ",
"ja476:13.3":	"तुय्हञ्चापि महाराज, ",
"ja476:13.4":	"मय्हञ्च रट्ठवड्ढन; ",
"ja476:13.5":	"अप्पेव नाम पस्सेमु, ",
"ja476:13.6":	"अहोरत्तानमच्चये”ति। ",
"ja476:14.1":	"जवनहंसजातकं ततियं। "
}	
