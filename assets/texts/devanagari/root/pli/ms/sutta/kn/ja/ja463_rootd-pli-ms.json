{	
"ja463:0.1":	"जातक ",
"ja463:0.2":	"एकादसकनिपात ",
"ja463:0.3":	"मातुपोसकवग्ग ",
"ja463:0.4":	"९। सुप्पारकजातक ",
"ja463:1.1":	"“उम्मुज्जन्ति निमुज्जन्ति, ",
"ja463:1.2":	"मनुस्सा खुरनासिका; ",
"ja463:1.3":	"सुप्पारकं तं पुच्छाम, ",
"ja463:1.4":	"समुद्दो कतमो अयं”। ",
"ja463:2.1":	"“कुरुकच्छा पयातानं, ",
"ja463:2.2":	"वाणिजानं धनेसिनं; ",
"ja463:2.3":	"नावाय विप्पनट्ठाय, ",
"ja463:2.4":	"खुरमालीति वुच्चति”। ",
"ja463:3.1":	"“यथा अग्गीव सुरियोव, ",
"ja463:3.2":	"समुद्दो पटिदिस्सति; ",
"ja463:3.3":	"सुप्पारकं तं पुच्छाम, ",
"ja463:3.4":	"समुद्दो कतमो अयं”। ",
"ja463:4.1":	"“कुरुकच्छा पयातानं, ",
"ja463:4.2":	"वाणिजानं धनेसिनं; ",
"ja463:4.3":	"नावाय विप्पनट्ठाय, ",
"ja463:4.4":	"अग्गिमालीति वुच्चति”। ",
"ja463:5.1":	"“यथा दधीव खीरंव, ",
"ja463:5.2":	"समुद्दो पटिदिस्सति; ",
"ja463:5.3":	"सुप्पारकं तं पुच्छाम, ",
"ja463:5.4":	"समुद्दो कतमो अयं”। ",
"ja463:6.1":	"“कुरुकच्छा पयातानं, ",
"ja463:6.2":	"वाणिजानं धनेसिनं; ",
"ja463:6.3":	"नावाय विप्पनट्ठाय, ",
"ja463:6.4":	"दधिमालीति वुच्चति”। ",
"ja463:7.1":	"“यथा कुसोव सस्सोव, ",
"ja463:7.2":	"समुद्दो पटिदिस्सति; ",
"ja463:7.3":	"सुप्पारकं तं पुच्छाम, ",
"ja463:7.4":	"समुद्दो कतमो अयं”। ",
"ja463:8.1":	"“कुरुकच्छा पयातानं, ",
"ja463:8.2":	"वाणिजानं धनेसिनं; ",
"ja463:8.3":	"नावाय विप्पनट्ठाय, ",
"ja463:8.4":	"कुसमालीति वुच्चति”। ",
"ja463:9.1":	"“यथा नळोव वेळूव, ",
"ja463:9.2":	"समुद्दो पटिदिस्सति; ",
"ja463:9.3":	"सुप्पारकं तं पुच्छाम, ",
"ja463:9.4":	"समुद्दो कतमो अयं”। ",
"ja463:10.1":	"“कुरुकच्छा पयातानं, ",
"ja463:10.2":	"वाणिजानं धनेसिनं; ",
"ja463:10.3":	"नावाय विप्पनट्ठाय, ",
"ja463:10.4":	"नळमालीति वुच्चति”। ",
"ja463:11.1":	"“महब्भयो भिंसनको, ",
"ja463:11.2":	"सद्दो सुय्यतिमानुसो; ",
"ja463:11.3":	"यथा सोब्भो पपातोव, ",
"ja463:11.4":	"समुद्दो पटिदिस्सति; ",
"ja463:11.5":	"सुप्पारकं तं पुच्छाम, ",
"ja463:11.6":	"समुद्दो कतमो अयं”। ",
"ja463:12.1":	"“कुरुकच्छा पयातानं, ",
"ja463:12.2":	"वाणिजानं धनेसिनं; ",
"ja463:12.3":	"नावाय विप्पनट्ठाय, ",
"ja463:12.4":	"बळवामुखीति वुच्चति। ",
"ja463:13.1":	"यतो सरामि अत्तानं, ",
"ja463:13.2":	"यतो पत्तोस्मि विञ्ञुतं; ",
"ja463:13.3":	"नाभिजानामि सञ्चिच्च, ",
"ja463:13.4":	"एकपाणम्पि हिंसितं; ",
"ja463:13.5":	"एतेन सच्चवज्जेन, ",
"ja463:13.6":	"सोत्थिं नावा निवत्ततू”ति। ",
"ja463:14.1":	"सुप्पारकजातकं नवमं। ",
"ja463:15.1":	"एकादसकनिपातं निट्ठितं। ",
"ja463:16.1":	"तस्सुद्दानं ",
"ja463:17.1":	"सिरिमातुसुपोसकनागवरो, ",
"ja463:17.2":	"पुन जुण्हक धम्ममुदयवरो; ",
"ja463:17.3":	"अथ पानि युधञ्चयको च दस- ",
"ja463:17.4":	"रथ संवर पारगतेन नवाति। "
}	
