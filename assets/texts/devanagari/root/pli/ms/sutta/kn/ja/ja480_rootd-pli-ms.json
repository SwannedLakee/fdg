{	
"ja480:0.1":	"जातक ",
"ja480:0.2":	"तेरसकनिपात ",
"ja480:0.3":	"अम्बवग्ग ",
"ja480:0.4":	"७। अकित्तिजातक ",
"ja480:1.1":	"“अकित्तिं दिस्वा सम्मन्तं, ",
"ja480:1.2":	"सक्को भूतपती ब्रवि; ",
"ja480:1.3":	"किं पत्थयं महाब्रह्मे, ",
"ja480:1.4":	"एको सम्मसि घम्मनि”। ",
"ja480:2.1":	"“दुक्खो पुनब्भवो सक्क, ",
"ja480:2.2":	"सरीरस्स च भेदनं; ",
"ja480:2.3":	"सम्मोहमरणं दुक्खं, ",
"ja480:2.4":	"तस्मा सम्मामि वासव”। ",
"ja480:3.1":	"“एतस्मिं ते सुलपिते, ",
"ja480:3.2":	"पतिरूपे सुभासिते; ",
"ja480:3.3":	"वरं कस्सप ते दम्मि, ",
"ja480:3.4":	"यं किञ्चि मनसिच्छसि”। ",
"ja480:4.1":	"“वरञ्चे मे अदो सक्क, ",
"ja480:4.2":	"सब्बभूतानमिस्सर; ",
"ja480:4.3":	"येन पुत्ते च दारे च, ",
"ja480:4.4":	"धनधञ्ञं पियानि च; ",
"ja480:4.5":	"लद्धा नरा न तप्पन्ति, ",
"ja480:4.6":	"सो लोभो न मयी वसे”। ",
"ja480:5.1":	"“एतस्मिं ते सुलपिते, ",
"ja480:5.2":	"पतिरूपे सुभासिते; ",
"ja480:5.3":	"वरं कस्सप ते दम्मि, ",
"ja480:5.4":	"यं किञ्चि मनसिच्छसि”। ",
"ja480:6.1":	"“वरञ्चे मे अदो सक्क, ",
"ja480:6.2":	"सब्बभूतानमिस्सर; ",
"ja480:6.3":	"खेत्तं वत्थुं हिरञ्ञञ्च, ",
"ja480:6.4":	"गवास्सं दासपोरिसं; ",
"ja480:6.5":	"येन जातेन जीयन्ति, ",
"ja480:6.6":	"सो दोसो न मयी वसे”। ",
"ja480:7.1":	"“एतस्मिं ते सुलपिते, ",
"ja480:7.2":	"पतिरूपे सुभासिते; ",
"ja480:7.3":	"वरं कस्सप ते दम्मि, ",
"ja480:7.4":	"यं किञ्चि मनसिच्छसि”। ",
"ja480:8.1":	"“वरञ्चे मे अदो सक्क, ",
"ja480:8.2":	"सब्बभूतानमिस्सर; ",
"ja480:8.3":	"बालं न पस्से न सुणे, ",
"ja480:8.4":	"न च बालेन संवसे; ",
"ja480:8.5":	"बालेनल्लापसल्लापं, ",
"ja480:8.6":	"न करे न च रोचये”। ",
"ja480:9.1":	"“किं नु ते अकरं बालो, ",
"ja480:9.2":	"वद कस्सप कारणं; ",
"ja480:9.3":	"केन कस्सप बालस्स, ",
"ja480:9.4":	"दस्सनं नाभिकङ्खसि”। ",
"ja480:10.1":	"“अनयं नयति दुम्मेधो, ",
"ja480:10.2":	"अधुरायं नियुञ्जति; ",
"ja480:10.3":	"दुन्नयो सेय्यसो होति, ",
"ja480:10.4":	"सम्मा वुत्तो पकुप्पति; ",
"ja480:10.5":	"विनयं सो न जानाति, ",
"ja480:10.6":	"साधु तस्स अदस्सनं”। ",
"ja480:11.1":	"“एतस्मिं ते सुलपिते, ",
"ja480:11.2":	"पतिरूपे सुभासिते; ",
"ja480:11.3":	"वरं कस्सप ते दम्मि, ",
"ja480:11.4":	"यं किञ्चि मनसिच्छसि”। ",
"ja480:12.1":	"“वरञ्चे मे अदो सक्क, ",
"ja480:12.2":	"सब्बभूतानमिस्सर; ",
"ja480:12.3":	"धीरं पस्से सुणे धीरं, ",
"ja480:12.4":	"धीरेन सह संवसे; ",
"ja480:12.5":	"धीरेनल्लापसल्लापं, ",
"ja480:12.6":	"तं करे तञ्च रोचये”। ",
"ja480:13.1":	"“किं नु ते अकरं धीरो, ",
"ja480:13.2":	"वद कस्सप कारणं; ",
"ja480:13.3":	"केन कस्सप धीरस्स, ",
"ja480:13.4":	"दस्सनं अभिकङ्खसि”। ",
"ja480:14.1":	"“नयं नयति मेधावी, ",
"ja480:14.2":	"अधुरायं न युञ्जति; ",
"ja480:14.3":	"सुनयो सेय्यसो होति, ",
"ja480:14.4":	"सम्मा वुत्तो न कुप्पति; ",
"ja480:14.5":	"विनयं सो पजानाति, ",
"ja480:14.6":	"साधु तेन समागमो”। ",
"ja480:15.1":	"“एतस्मिं ते सुलपिते, ",
"ja480:15.2":	"पतिरूपे सुभासिते; ",
"ja480:15.3":	"वरं कस्सप ते दम्मि, ",
"ja480:15.4":	"यं किञ्चि मनसिच्छसि”। ",
"ja480:16.1":	"“वरञ्चे मे अदो सक्क, ",
"ja480:16.2":	"सब्बभूतानमिस्सर; ",
"ja480:16.3":	"ततो रत्या विवसाने, ",
"ja480:16.4":	"सूरियुग्गमनं पति; ",
"ja480:16.5":	"दिब्बा भक्खा पातुभवेय्युं, ",
"ja480:16.6":	"सीलवन्तो च याचका। ",
"ja480:17.1":	"ददतो मे न खीयेथ, ",
"ja480:17.2":	"दत्वा नानुतपेय्यहं; ",
"ja480:17.3":	"ददं चित्तं पसादेय्यं, ",
"ja480:17.4":	"एतं सक्क वरं वरे”। ",
"ja480:18.1":	"“एतस्मिं ते सुलपिते, ",
"ja480:18.2":	"पतिरूपे सुभासिते; ",
"ja480:18.3":	"वरं कस्सप ते दम्मि, ",
"ja480:18.4":	"यं किञ्चि मनसिच्छसि”। ",
"ja480:19.1":	"“वरञ्चे मे अदो सक्क, ",
"ja480:19.2":	"सब्बभूतानमिस्सर; ",
"ja480:19.3":	"न मं पुन उपेय्यासि, ",
"ja480:19.4":	"एतं सक्क वरं वरे”। ",
"ja480:20.1":	"“बहूहि वतचरियाहि, ",
"ja480:20.2":	"नरा च अथ नारियो; ",
"ja480:20.3":	"दस्सनं अभिकङ्खन्ति, ",
"ja480:20.4":	"किं नु मे दस्सने भयं”। ",
"ja480:21.1":	"“तं तादिसं देववण्णं, ",
"ja480:21.2":	"सब्बकामसमिद्धिनं; ",
"ja480:21.3":	"दिस्वा तपो पमज्जेय्यं, ",
"ja480:21.4":	"एतं ते दस्सने भयन्”ति। ",
"ja480:22.1":	"अकित्तिजातकं सत्तमं। "
}	
