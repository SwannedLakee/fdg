{
  "sn1.23:0.1": "Связанные Наставления 1.23",
  "sn1.23:0.2": "Глава Сатти",
  "sn1.23:0.3": "Узел",
  "sn1.23:1.1": "«Спутан снаружи и спутан внутри,",
  "sn1.23:1.2": "Поколение – спутанный узел.",
  "sn1.23:1.3": "Спрошу я тебя, о Готама,",
  "sn1.23:1.4": "Кто узел сей может распутать?»",
  "sn1.23:2.1": "[Благословенный]: «Мудрый, окрепший в нравственности,",
  "sn1.23:2.2": "Ум развивающий и мудрость,",
  "sn1.23:2.3": "Старательный и рассудительный монах:",
  "sn1.23:2.4": "Вот может кто распутать этот узел.",
  "sn1.23:3.1": "В ком злость и жажда",
  "sn1.23:3.2": "И невежество были удалены,",
  "sn1.23:3.3": "Архаты, загрязнения разбившие:",
  "sn1.23:3.4": "Вот узел у кого распутан.",
  "sn1.23:4.1": "Где имя-форма угасает,",
  "sn1.23:4.2": "И без остатка исчезает,",
  "sn1.23:4.3": "И нет где столкновений с восприятием форм:",
  "sn1.23:4.4": "Вот где разрублен узел сей."
}