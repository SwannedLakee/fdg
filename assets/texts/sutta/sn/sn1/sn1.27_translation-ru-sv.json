{
  "sn1.27:0.1": "Связанные Наставления 1.27",
  "sn1.27:0.2": "Глава Сатти",
  "sn1.27:0.3": "Потоки",
  "sn1.27:1.1": "«Откуда потоки возвращаются назад?",
  "sn1.27:1.2": "Где цикл перестает вращаться?",
  "sn1.27:1.3": "Где имя и форма",
  "sn1.27:1.4": "прекращаются без остатка?»",
  "sn1.27:2.1": "«Где вода и земля,",
  "sn1.27:2.2": "огонь и воздух не находят опоры—",
  "sn1.27:2.3": "оттуда потоки возвращаются назад;",
  "sn1.27:2.4": "там цикл перестает вращаться;",
  "sn1.27:2.5": "и именно там имя и форма",
  "sn1.27:2.6": "прекращаются без остатка»."
}