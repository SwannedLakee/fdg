436\u0434\u044b (\u0441\u0430\u043a\u0430\u0434\u0430\u0433\u0430\u043c\u0438), \u0422\u0440\u0435\u0442\u0438\u0439 \u043f\u0443\u0442\u044c \u0438\u043b\u0438 \u0411\u043e\u043b\u044c\u0448\u0435 \u043d\u0435 \u0432\u043e\u0437\u0432\u0440\u0430\u0449\u0430\u044e\u0449\u0438\u0439\u0441\u044f (\u0430\u043d\u0430\u0433\u0430\u043c\u0438) \u0438 \u043d\u0430\u043a\u043e\u043d\u0435\u0446 \u0427\u0435\u0442\u0432\u0451\u0440\u0442\u044b\u0439 \u043f\u0443\u0442\u044c, \u0421\u0432\u044f\u0442\u0435\u0439\u0448\u0438\u0439, \u0421\u0432\u044f\u0442\u043e\u0439 \u0438\u043b\u0438 \u041f\u043e\u0431\u0435\u0434\u043e\u043d\u043e\u0441\u043d\u044b\u0439 (\u0430\u0440\u0430\u0445\u0430\u0442, \u0430\u0440\u0445\u0430\u0442, \u0430\u0440\u0430\u0445\u0430\u043d\u0442 \u0438\u043b\u0438 \u0430\u0440\u0445\u0430\u043d\u0442 \u2014 \u043f\u0438\u0448\u0438\u0442\u0435 \u0442\u0430\u043a, \u043a\u0430\u043a \u0432\u0430\u043c \u043d\u0440\u0430\u0432\u0438\u0442\u0441\u044f). \u0422\u0435\u0440\u043c\u0438\u043d\u044b \u00ab\u0432\u043e\u0437\u0432\u0440\u0430\u0449\u0430\u044e\u0449\u0438\u0439\u0441\u044f \u043e\u0434\u043d\u0430\u0436\u0434\u044b\u00bb \u0438 \u00ab\u0431\u043e\u043b\u044c\u0448\u0435 \u043d\u0435 \u0432\u043e\u0437\u0432\u0440\u0430\u0449\u0430\u044e\u0449\u0438\u0439\u0441\u044f\u00bb \u0441\u0432\u044f\u0437\u0430\u043d\u044b \u0441 \u0434\u043e\u0433\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u043c\u0438 \u0443\u0442\u0432\u0435\u0440\u0436\u0434\u0435\u043d\u0438\u044f\u043c\u0438, \u0431\u0443\u0434\u0442\u043e \u0434\u043e\u0441\u0442\u0438\u0433\u0448\u0438\u0435 \u0432\u0442\u043e\u0440\u043e\u0433\u043e \u043f\u0443\u0442\u0438 \u043c\u043e\u0433\u0443\u0442 \u0440\u043e\u0434\u0438\u0442\u044c\u0441\u044f \u043b\u0438\u0448\u044c \u0440\u0430\u0437, \u043f\u0440\u0435\u0436\u0434\u0435 \u0447\u0435\u043c \u0434\u043e\u0441\u0442\u0438\u0433\u043d\u0443\u0442 \u0443\u0440\u043e\u0432\u043d\u044f \u0430\u0440\u0445\u0430\u0442\u0430, \u0438 \u044d\u0442\u043e \u0440\u043e\u0436\u0434\u0435\u043d\u0438\u0435 \u0442\u043e\u0447\u043d\u043e \u043d\u0435 \u0431\u0443\u0434\u0435\u0442 \u0438\u043c\u0435\u0442\u044c \u043c\u0435\u0441\u0442\u043e \u0432 \u043d\u0438\u0437\u0448\u0438\u0445 \u043c\u0438\u0440\u0430\u0445 (\u0430\u0434\u0430\u0445 \u0438 \u043c\u0438\u0440\u0430\u0445 \u0433\u043e\u043b\u043e\u0434\u043d\u044b\u0445 \u0434\u0443\u0445\u043e\u0432 \u0438\u043b\u0438 \u0436\u0438\u0432\u043e\u0442\u043d\u044b\u0445), \u0438 \u0431\u0443\u0434\u0442\u043e \u0442\u0435, \u043a\u0442\u043e \u043d\u0430\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u043d\u0430 \u0442\u0440\u0435\u0442\u044c\u0435\u043c \u043f\u0443\u0442\u0438, \u043d\u0435 \u0434\u043e\u0441\u0442\u0438\u0433\u043d\u0443\u0432 \u0443\u0440\u043e\u0432\u043d\u044f \u0430\u0440\u0445\u0430\u0442\u0430 \u0432 \u044d\u0442\u043e\u0439 \u0436\u0438\u0437\u043d\u0438, \u0432 \u0445\u0443\u0434\u0448\u0435\u043c \u0441\u043b\u0443\u0447\u0430\u0435 \u0440\u043e\u0434\u044f\u0442\u0441\u044f \u043d\u0430 \u043d\u0435\u0431\u0435\u0441\u0430\u0445, \u0433\u0434\u0435 \u0443\u0441\u043b\u043e\u0432\u0438\u044f \u0434\u043b\u044f \u0434\u043e\u0441\u0442\u0438\u0436\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u0441\u0432\u0435\u0442\u043b\u0435\u043d\u0438\u044f \u043e\u043f\u0442\u0438\u043c\u0430\u043b\u044c\u043d\u044b. \u041e\u0434\u043d\u0430\u043a\u043e \u044f\u0434\u0440\u043e\u043c \u043c\u043e\u0434\u0435\u043b\u0438 \u0447\u0435\u0442\u044b\u0440\u0451\u0445 \u043f\u0443\u0442\u0435\u0439 \u0442\u0445\u0435\u0440\u0430\u0432\u0430\u0434\u044b \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0434\u043e\u0433\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u0443\u0442\u0432\u0435\u0440\u0436\u0434\u0435\u043d\u0438\u0435, \u0431\u0443\u0434\u0442\u043e \u043f\u0440\u043e\u0441\u0432\u0435\u0442\u043b\u0435\u043d\u0438\u0435 \u0441\u0432\u044f\u0437\u0430\u043d\u043e \u0441 \u043f\u043e\u0441\u043b\u0435\u0434\u043e\u04