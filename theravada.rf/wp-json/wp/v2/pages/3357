043e \u0431\u044b \u043b\u044e\u0434\u0438 \u0437\u043d\u0430\u043b\u0438, \u0447\u0442\u043e \u0442\u044b \u0445\u043e\u0440\u043e\u0448\u0438\u0439 \u0447\u0435\u043b\u043e\u0432\u0435\u043a, \u043d\u0435 \u043e\u043f\u0430\u0441\u043d\u044b\u0439, \u043d\u0430\u0434\u043e \u0433\u043e\u0432\u043e\u0440\u0438\u0442\u044c, \u0447\u0442\u043e \u0442\u044b \u0431\u0430\u0431\u043d\u0438\u043a\u00bb. \u041a\u043e \u043c\u043d\u0435 \u043c\u043d\u043e\u0433\u043e \u043b\u044e\u0434\u0435\u0439 \u043f\u0440\u0438\u0445\u043e\u0434\u0438\u043b\u043e. \u041a\u043e\u0433\u0434\u0430 \u043e\u043d\u0438 \u0441\u0442\u0443\u0447\u0430\u043b\u0438 \u0432 \u0434\u0432\u0435\u0440\u044c, \u043e\u043d \u043e\u0442\u043a\u0440\u044b\u0432\u0430\u043b \u0438 \u0441\u0440\u0430\u0437\u0443 \u0433\u043e\u0432\u043e\u0440\u0438\u043b: \u00ab\u041c\u0435\u043d\u044f \u0437\u043e\u0432\u0443\u0442 \u0411\u0430\u043e, \u044f \u2013 \u0431\u0430\u0431\u043d\u0438\u043a\u00bb. \u0418 \u043b\u044e\u0434\u0438 \u0441\u043c\u0435\u044f\u043b\u0438\u0441\u044c \u042f \u043d\u0435 \u0441\u043b\u044b\u0448\u0430\u043b, \u0447\u0442\u043e \u043e\u043d \u0433\u043e\u0432\u043e\u0440\u0438\u0442. \u0410 \u043a\u043e\u0433\u0434\u0430 \u0443\u0441\u043b\u044b\u0448\u0430\u043b \u0442\u043e, \u0441\u043f\u0440\u043e\u0441\u0438\u043b \u0435\u0433\u043e, \u043f\u043e\u0447\u0435\u043c\u0443 \u043e\u043d \u0442\u0430\u043a \u0433\u043e\u0432\u043e\u0440\u0438\u0442. \u041e\u043d \u043e\u0442\u0432\u0435\u0442\u0438\u043b: \u00ab\u041c\u043e\u0439 \u0434\u0440\u0443\u0433 \u043c\u0435\u043d\u044f \u043d\u0430\u0443\u0447\u0438\u043b, \u043e\u043d \u0441\u043a\u0430\u0437\u0430\u043b, \u0447\u0442\u043e \u0435\u0441\u043b\u0438 \u0442\u044b \u0445\u043e\u0447\u0435\u0448\u044c \u0441\u043a\u0430\u0437\u0430\u0442\u044c, \u0447\u0442\u043e \u0442\u044b \u0445\u043e\u0440\u043e\u0448\u0438\u0439 \u0447\u0435\u043b\u043e\u0432\u0435\u043a, \u0442\u043e \u043d\u0430\u0434\u043e \u0433\u043e\u0432\u043e\u0440