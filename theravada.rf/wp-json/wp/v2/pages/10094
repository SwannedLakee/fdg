43a \u0431\u0443\u0434\u0442\u043e \u0432\u043e\u0442-\u0432\u043e\u0442 \u0443\u0442\u043e\u043d\u0435\u0442 \u0438\u043b\u0438 \u0441\u043e\u043b\u044c\u0435\u0442\u0441\u044f \u0441 \u043d\u0438\u043c. \u041a \u044d\u0442\u043e\u043c\u0443 \u0432\u0440\u0435\u043c\u0435\u043d\u0438 \u043c\u0435\u0434\u0438\u0442\u0430\u0442\u043e\u0440 \u0443\u0436\u0435 \u043f\u0440\u0435\u043e\u0434\u043e\u043b\u0435\u043b \u043f\u0440\u0435\u043f\u044f\u0442\u0441\u0442\u0432\u0438\u044f \u041d\u0438\u0432\u0430\u0440\u0430\u043d\u0430 \u0438 \u0431\u043b\u0438\u0437\u043e\u043a \u043a \u0441\u0442\u0430\u0431\u0438\u043b\u044c\u043d\u043e\u043c\u0443 \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u044e \u043f\u043e\u0433\u043b\u043e\u0449\u0435\u043d\u0438\u044f. \u0417\u0434\u0435\u0441\u044c \u0443\u043c \u0434\u043e\u0441\u0442\u0438\u0433 \u043e\u0447\u0435\u043d\u044c \u0442\u043e\u043d\u043a\u043e\u0433\u043e \u0438 \u043f\u043e\u0445\u043e\u0436\u0435\u0433\u043e \u043d\u0430 \u0441\u043e\u043d \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u044f. \u0415\u0441\u043b\u0438 \u043c\u0435\u0434\u0438\u0442\u0430\u0442\u043e\u0440 \u043d\u0435 \u0431\u0443\u0434\u0435\u0442 \u043e\u0441\u0442\u043e\u0440\u043e\u0436\u0435\u043d, \u043e\u043d \u043c\u043e\u0436\u0435\u0442 \u043f\u0440\u043e\u0441\u0442\u043e \u0437\u0430\u0441\u043d\u0443\u0442\u044c. \u0417\u0434\u0435\u0441\u044c \u043d\u0443\u0436\u043d\u043e \u0431\u044b\u0442\u044c \u0447\u0440\u0435\u0437\u0432\u044b\u0447\u0430\u0439\u043d\u043e \u0432\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u043c, \u0447\u0442\u043e\u0431\u044b \u0441 \u043e\u0434\u043d\u043e\u0439 \u0441\u0442\u043e\u0440\u043e\u043d\u044b \u043f\u043e\u0434\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u0442\u044c \u043f\u043e\u0442\u043e\u043a \u041c\u0435\u0442\u0442\u044b, \u0438 \u0432 \u0442\u043e \u0436\u0435 \u0432\u0440\u0435\u043c\u044f \u0441\u043b\u0435\u0434\u0438\u0442\u044c \u0437\u0430 \u0442\u0435\u043c, \u0447\