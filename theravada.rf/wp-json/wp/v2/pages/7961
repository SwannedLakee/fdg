0441\u044f \u0432\u043c\u0435\u0441\u0442\u0435 \u0438\u0437-\u0437\u0430 \u0443\u0434\u043e\u0431\u0441\u0442\u0432\u0430, \u0430 \u0440\u0430\u0434\u043e\u0441\u0442\u044c \u0434\u0430\u0432\u043d\u043e \u0443\u0448\u043b\u0430. \u041a\u043e\u0433\u0434\u0430 \u0432\u044b \u043f\u043e\u043d\u0438\u043c\u0430\u0435\u0442\u0435, \u0447\u0442\u043e \u0441\u043d\u0430\u0447\u0430\u043b\u0430 \u043f\u043e\u043b\u0443\u0447\u0430\u0435\u0442\u0435 \u0443\u0434\u043e\u0432\u043e\u043b\u044c\u0441\u0442\u0432\u0438\u0435 \u0438 \u0447\u0442\u043e \u0437\u0430\u043f\u043b\u0430\u0442\u0438\u0442\u0435 \u0437\u0430 \u044d\u0442\u043e \u043f\u043e\u0437\u0436\u0435, \u0442\u043e \u0447\u0443\u0432\u0441\u0442\u0432\u0443\u0435\u0442\u0435 \u043d\u0438\u0431\u0431\u0438\u0434\u0443 \u043a\u043e \u0432\u0441\u0435\u0439 \u044d\u0442\u043e\u0439 \u0447\u0443\u0432\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u0441\u0444\u0435\u0440\u0435: \u043a \u0442\u0435\u043b\u0443, \u043a \u043e\u0442\u043d\u043e\u0448\u0435\u043d\u0438\u044f\u043c, \u043a \u043f\u0440\u0438\u0445\u043e\u0434\u0443 \u0438 \u0443\u0445\u043e\u0434\u0443, \u0434\u0430\u0436\u0435 \u043a \u043f\u043e\u0441\u0442\u0440\u043e\u0435\u043d\u0438\u044e \u043c\u043e\u043d\u0430\u0441\u0442\u044b\u0440\u0435\u0439. \u0412\u0441\u0435 \u044d\u0442\u043e \u0441\u043a\u043e\u0440\u043e \u0438\u0441\u0447\u0435\u0437\u043d\u0435\u0442. \u0414\u0430\u0436\u0435 \u0432\u0435\u043b\u0438\u043a\u0430\u044f \u0437\u0435\u043c\u043b\u044f \u0432 \u043a\u043e\u043d\u0446\u0435 \u043a\u043e\u043d\u0446\u043e\u0432 \u0431\u0443\u0434\u0435\u0442 \u043f\u043e\u0433\u043b\u043e\u0449\u0435\u043d\u0430 \u0441\u043e\u043b\u043d\u0446\u0435\u043c, \u0438 \u0442\u043e\u0433\u0434\u0430 \u043d\u0435 \u043e\u0441\u0442\u0430\u043d\u0435\u0442\u0441\u044f \u043d\u0438\u0447\u0435\u0433\u043e: \u043d\u0438 \u0437\u0430\u043f\u0438\u0441\u0435\u0439 \u043e \u043b\u044e\u0434\u044f\u0445, \u043d\u0438 \u0412\u0435\u043b\u0438\u043a\u04