44b\u0445 \u041f\u0440\u0438\u0437\u0440\u0430\u043a\u043e\u0432; \u043e\u043d \u043f\u0435\u0440\u0435\u0440\u043e\u0434\u0438\u043b\u0441\u044f \u0432 \u043c\u0438\u0440\u0435 \u0411\u0440\u0430\u043c\u043c, \u0443 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043d\u0435\u0442 \u0447\u0443\u0432\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0445 \u043e\u0440\u0433\u0430\u043d\u043e\u0432; \u043e\u043d \u043f\u0435\u0440\u0435\u0440\u043e\u0434\u0438\u043b\u0441\u044f \u0432 \u0434\u0440\u0435\u043c\u0443\u0447\u0435\u0439 \u0441\u0442\u0440\u0430\u043d\u0435 \u0432\u0430\u0440\u0432\u0430\u0440\u043e\u0432; \u0435\u0433\u043e \u0443\u043c \u043d\u0435 \u043e\u0442\u043a\u0440\u044b\u0442 \u0434\u043b\u044f \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u044f \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e\u0433\u043e \u0432\u043e\u0437\u0437\u0440\u0435\u043d\u0438\u044f \u043e \u0431\u0443\u0434\u0443\u044e\u0449\u0438\u0445 \u043f\u0435\u0440\u0435\u0440\u043e\u0436\u0434\u0435\u043d\u0438\u044f\u0445 \u0438 \u0417\u0430\u043a\u043e\u043d\u0435 \u041a\u0430\u0440\u043c\u044b (\u041f\u0440\u0438\u0447\u0438\u043d\u043d\u043e-\u0441\u043b\u0435\u0434\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u0441\u0432\u044f\u0437\u0438); \u0435\u0433\u043e \u0443\u043c \u0442\u0443\u043f \u0438 \u043d\u0435 \u0441\u043f\u043e\u0441\u043e\u0431\u0435\u043d \u043f\u043e\u043d\u044f\u0442\u044c \u0423\u0447\u0435\u043d\u0438\u044f; \u043f\u043e\u044f\u0432\u0438\u0432\u0448\u0438\u0439\u0441\u044f \u0411\u0443\u0434\u0434\u0430 \u043d\u0435 \u0443\u0447\u0438\u0442&#187;.<\/span><\/p>\n<p><span style=\"font-size: 20px;\">\u0410(9):12 &#187; \u0423\u043c\u0438\u0440\u0430\u044e\u0442 \u0431\u0435\u0437 \u0432\u043b\u0435\u0447\u0435\u043d\u0438\u044f \u043a \u0447\u0435\u043c\u0443 \u0431\u044b \u0442\u043e \u043d\u0438 \u0431\u044b\u043b\u043e \u0432 \u044d\u0442\u043e\u043c \u043c\u0438\u0440\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u043f\u043e\u043b\u043d\u043e\u04