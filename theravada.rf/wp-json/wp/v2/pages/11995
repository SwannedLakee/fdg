\/>\n\u041a\u0442\u043e \u0432\u0441\u0451 \u044d\u0442\u043e \u043d\u0435 \u0441\u0447\u0438\u0442\u0430\u0435\u0442 \u0434\u043b\u044f \u0441\u0435\u0431\u044f \u0442\u0440\u0443\u0434\u043d\u044b\u043c, \u043a\u0442\u043e \u0434\u0443\u043c\u0430\u0435\u0442 &#171;\u044f \u0442\u0430\u043a \u043f\u0435\u0440\u0435\u0441\u0435\u043a\u0430\u044f \u0438 \u043f\u0440\u043e\u0445\u043e\u0434\u044f \u0434\u043e\u0441\u0442\u0438\u0433\u043d\u0443 \u0442\u043e\u0433\u043e \u0431\u0435\u0440\u0435\u0433\u0430&#187;, \u043a\u0442\u043e \u043e\u0431\u043b\u0430\u0434\u0430\u0435\u0442 \u044d\u0442\u0438\u043c \u043e\u0433\u0440\u043e\u043c\u043d\u044b\u043c \u0436\u0435\u043b\u0430\u043d\u0438\u0435\u043c, \u0441\u0442\u0440\u0435\u043c\u043b\u0435\u043d\u0438\u0435\u043c, \u0443\u0441\u0435\u0440\u0434\u0438\u0435\u043c \u0438 \u043f\u043e\u0438\u0441\u043a\u043e\u043c &#8212; \u0435\u0433\u043e \u0436\u0435\u043b\u0430\u043d\u0438\u0435 \u0438\u0441\u043f\u043e\u043b\u043d\u0438\u0442\u0441\u044f, \u0438 \u043d\u0438\u043a\u0430\u043a \u0438\u043d\u0430\u0447\u0435.<br \/>\n\u0410\u0441\u043a\u0435\u0442 \u0421\u0443\u043c\u0435\u0434\u0445\u0430 \u0436\u0435 \u0441\u043e\u0431\u0440\u0430\u0432 [\u0432 \u0441\u0435\u0431\u0435?] \u0432\u0441\u0435 \u044d\u0442\u0438 8 \u043a\u0430\u0447\u0435\u0441\u0442\u0432, \u043f\u0440\u0438\u043d\u044f\u043b \u0440\u0435\u0448\u0435\u043d\u0438\u0435 \u0441\u0442\u0430\u0442\u044c \u0411\u0443\u0434\u0434\u043e\u0439 \u0438 \u043b\u0451\u0433.<br \/>\n\u0422\u0435\u043c \u0432\u0440\u0435\u043c\u0435\u043d\u0435\u043c \u0431\u043b\u0430\u0433\u043e\u0441\u043b\u043e\u0432\u0435\u043d\u043d\u044b\u0439 \u0414\u0438\u043f\u0430\u043d\u043a\u0430\u0440\u0430 \u0434\u043e\u0448\u0451\u043b \u0434\u043e \u044d\u0442\u043e\u0433\u043e \u043c\u0435\u0441\u0442\u0430 \u0438 \u043e\u0441\u0442\u0430\u043d\u043e\u0432\u0438\u043b\u0441\u044f \u0432\u043e\u0437\u043b\u0435 \u0433\u043e\u043b\u043e\u0432\u044b \u043e\u0442\u0448\u0435\u043b\u