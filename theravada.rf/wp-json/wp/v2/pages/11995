r \/>\n\u0418 \u043a\u0430\u043a \u0432 \u043c\u043e\u043c\u0435\u043d\u0442 \u0435\u0433\u043e \u0437\u0430\u0447\u0430\u0442\u0438\u044f, \u0442\u0430\u043a \u0438 \u0432 \u043c\u043e\u043c\u0435\u043d\u0442 \u0435\u0433\u043e \u0440\u043e\u0436\u0434\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u044f\u0432\u0438\u043b\u0438\u0441\u044c \u0442\u0440\u0438\u0434\u0446\u0430\u0442\u044c \u0434\u0432\u0430 \u0437\u043d\u0430\u043a\u0430-\u043f\u0440\u0435\u0434\u0432\u0435\u0441\u0442\u043d\u0438\u043a\u0430.<br \/>\n\u0412 \u0442\u043e\u0442 \u0436\u0435 \u043c\u043e\u043c\u0435\u043d\u0442, \u043a\u043e\u0433\u0434\u0430 \u043d\u0430\u0448 \u0411\u043e\u0434\u0445\u0438\u0441\u0430\u0442\u0442\u0430 \u0440\u043e\u0434\u0438\u043b\u0441\u044f \u0432 \u0440\u043e\u0449\u0435 \u041b\u0443\u043c\u0431\u0438\u043d\u0438, \u0440\u043e\u0434\u0438\u043b\u0430\u0441\u044c \u043c\u0430\u0442\u044c \u0420\u0430\u0445\u0443\u043b\u044b, \u0441\u0442\u0430\u0440\u0448\u0438\u0439 \u043c\u043e\u043d\u0430\u0445 \u0410\u043d\u0430\u043d\u0434\u0430, \u0441\u043b\u0443\u0433\u0430 \u0427\u0445\u0430\u043d\u043d\u0430, \u043c\u0438\u043d\u0438\u0441\u0442\u0440 \u041a\u0430\u043b\u0443\u0434\u0430\u0439\u0438, \u043a\u043e\u0440\u043e\u043b\u0435\u0432\u0441\u043a\u0438\u0439 \u043a\u043e\u043d\u044c \u041a\u0430\u043d\u0442\u0445\u0430\u043a\u0430. \u0412 \u0442\u043e \u0436\u0435 \u0432\u0440\u0435\u043c\u044f \u0437\u0430\u0440\u043e\u0434\u0438\u043b\u043e\u0441\u044c \u0432\u0435\u043b\u0438\u043a\u043e\u0435 \u0434\u0435\u0440\u0435\u0432\u043e \u0411\u043e\u0434\u0445\u0438 \u0438 \u043d\u0430 \u0441\u0432\u0435\u0442 \u043f\u043e\u044f\u0432\u0438\u043b\u0438\u0441\u044c \u0447\u0435\u0442\u044b\u0440\u0435 \u0441\u043e\u0441\u0443\u0434\u0430, \u043f\u043e\u043b\u043d\u044b\u0435 \u0434\u0440\u0430\u0433\u043e\u0446\u0435\u043d\u043d\u043e\u0441\u0442\u0435\u0439.<br \/>\n\u041f\u0435\u0440\u0432\u044b\u0439 \u0438\u0437 \u0441\u043e\u0441\u0443\u0434\u043e\u0432 \u0431\u04