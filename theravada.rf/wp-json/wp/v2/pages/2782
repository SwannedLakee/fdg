44f. \u0425\u043e\u0440\u043e\u0448\u043e? \u0414\u044b\u0445\u0430\u043d\u0438\u0435 &#8212; \u044d\u0442\u043e \u043e\u0434\u043d\u043e, \u0441\u0432\u0435\u0442 &#8212; \u0434\u0440\u0443\u0433\u043e\u0435. \u042d\u0442\u043e \u0434\u0432\u0430 \u0440\u0430\u0437\u043d\u044b\u0445 \u043e\u0431\u044a\u0435\u043a\u0442\u0430. \u042d\u0442\u043e \u043d\u0435 \u043e\u0434\u0438\u043d \u043e\u0431\u044a\u0435\u043a\u0442. \u0412\u0430\u043c \u043d\u0443\u0436\u043d\u043e \u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u043e\u0434\u043d\u0443 \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u043d\u043e\u0441\u0442\u044c \u0443\u043c\u0430. \u041d\u0435 \u0434\u0432\u0435 \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u0443\u043c\u0430. \u041f\u043e\u043d\u044f\u0442\u043d\u043e? \u041d\u0435 \u0440\u0430\u0437\u043d\u043e\u043e\u0431\u0440\u0430\u0437\u0438\u0435 \u0432\u043e\u0441\u043f\u0440\u0438\u044f\u0442\u0438\u044f, \u043d\u043e \u0435\u0434\u0438\u043d\u0441\u0442\u0432\u043e \u0432\u043e\u0441\u043f\u0440\u0438\u044f\u0442\u0438\u044f. \u041c\u044b \u0443\u0445\u043e\u0434\u0438\u043c \u043e\u0442 \u0440\u0430\u0437\u043d\u043e\u043e\u0431\u0440\u0430\u0437\u0438\u044f \u043c\u0438\u0440\u0430, \u043c\u043d\u043e\u0433\u043e\u043e\u0431\u0440\u0430\u0437\u0438\u044f \u043c\u0438\u0440\u0430, \u0432\u0441\u0435\u0445 \u0432\u0435\u0449\u0435\u0439 \u0432 \u043c\u0438\u0440\u0435, \u043e\u0442 \u0432\u0441\u0435\u0445 \u044d\u0442\u0438\u0445 \u0432\u0435\u0449\u0435\u0439. \u041c\u044b \u0432\u0442\u044f\u0433\u0438\u0432\u0430\u0435\u043c \u0438\u0445 \u043e\u0431\u0440\u0430\u0442\u043d\u043e \u0432\u043e\u0432\u043d\u0443\u0442\u0440\u044c, \u0442\u044f\u043d\u0435\u043c \u0432\u0441\u0435 \u0448\u0435\u0441\u0442\u044c \u0447\u0443\u0432\u0441\u0442\u0432. \u041c\u044b \u0432\u044b\u0442\u044f\u0433\u0438\u0432\u0430\u0435\u043c \u0442\u043e\u043b\u044c\u043a\u043e \u043e\u0434\u043d\u043e \u0447\u0443\u0432\u0441\u