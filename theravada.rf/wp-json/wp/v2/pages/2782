43e\u0432\u0435\u043a \u0431\u0443\u0434\u0435\u0442 \u0441\u0447\u0430\u0441\u0442\u043b\u0438\u0432, \u043f\u0443\u0441\u0442\u044c \u043b\u044e\u0431\u0438\u043c\u044b\u0439 \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u0431\u0443\u0434\u0435\u0442 \u0441\u0447\u0430\u0441\u0442\u043b\u0438\u0432,\u00a0\u043f\u0443\u0441\u0442\u044c \u043b\u044e\u0431\u0438\u043c\u044b\u0439 \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u0431\u0443\u0434\u0435\u0442 \u0441\u0447\u0430\u0441\u0442\u043b\u0438\u0432. \u041f\u0440\u043e\u0441\u0442\u043e \u0432\u0441\u0442\u0430\u0432\u044c\u0442\u0435 \u0438\u043c\u044f \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430 \u0432 \u044d\u0442\u0438 \u0441\u043b\u043e\u0432\u0430 \u041c\u0435\u0442\u0442\u044b, \u0441\u043e\u0445\u0440\u0430\u043d\u044f\u044f \u0443\u043c \u0443\u0441\u0442\u043e\u0439\u0447\u0438\u0432\u044b\u043c \u0438 \u0441\u043e\u0441\u0440\u0435\u0434\u043e\u0442\u043e\u0447\u0435\u043d\u043d\u044b\u043c \u043d\u0430 \u0432\u0441\u0435\u043c \u0442\u0435\u043b\u0435.<\/span><\/em><\/p>\n<p><em><span style=\"font-size: 20px;\">\u0418 \u0437\u0430\u0442\u0435\u043c \u043f\u0435\u0440\u0435\u0439\u0434\u0438\u0442\u0435 \u043a \u043d\u0435\u0439\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u043c\u0443 \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0443, \u043e\u043f\u044f\u0442\u044c \u0436\u0435, \u0441\u043e\u0445\u0440\u0430\u043d\u044f\u044f \u0444\u043e\u043a\u0443\u0441 \u0443\u043c\u0430 \u0442\u0430\u043a\u0438\u043c \u0436\u0435, \u043a\u0430\u043a \u043e\u043d \u0435\u0441\u0442\u044c, \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044f \u0438\u043c\u044f \u043d\u0435\u0439\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430 \u0432 \u0441\u043b\u043e\u0432\u0430\u0445 \u041b\u044e\u0431\u044f\u0449\u0435\u0439-\u0434\u043e\u0431\u0440\u043e\u0442\u044b, \u043f\u0443\u0441\u0442\u044c \u044d\u0442\u043e\u0442 \u043d\u0435\u0439\u0442\u0440\u0430\u043b\u044c\u043d\