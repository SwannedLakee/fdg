043b\u044e\u0434\u0435\u043d\u0438\u0435 \u0437\u0430 \u043f\u043e\u0434\u044a\u0435\u043c\u043e\u043c \u0438 \u043f\u0430\u0434\u0435\u043d\u0438\u0435\u043c \u0436\u0438\u0432\u043e\u0442\u0430. \u0415\u0441\u043b\u0438 \u043f\u0440\u0438 \u044d\u0442\u043e\u043c \u0432\u043e\u0437\u043d\u0438\u043a\u043d\u0435\u0442 \u0431\u043e\u043b\u044c, \u0443\u0442\u043e\u043c\u043b\u0435\u043d\u0438\u0435, \u0437\u0443\u0434 \u0438\u043b\u0438 \u043a\u0430\u043a\u043e\u0435-\u0442\u043e \u0438\u043d\u043e\u0435 \u043e\u0449\u0443\u0449\u0435\u043d\u0438\u0435, \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u043e\u0442\u043c\u0435\u0447\u0430\u0439\u0442\u0435 \u043a\u0430\u0436\u0434\u043e\u0435 \u0438\u0437 \u043d\u0438\u0445. \u041e\u0442\u043c\u0435\u0447\u0430\u0439\u0442\u0435 \u0442\u0430\u043a\u0436\u0435 \u0432\u0441\u0435 \u0447\u0443\u0432\u0441\u0442\u0432\u0430, \u043c\u044b\u0441\u043b\u0438, \u0438\u0434\u0435\u0438, \u0441\u043e\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f, \u0440\u0430\u0437\u043c\u044b\u0448\u043b\u0435\u043d\u0438\u044f, \u0432\u0441\u0435 \u0434\u0432\u0438\u0436\u0435\u043d\u0438\u044f \u043a\u0438\u0441\u0442\u0435\u0439, \u0440\u0443\u043a, \u043d\u043e\u0433, \u0442\u0435\u043b\u0430. \u0415\u0441\u043b\u0438 \u043d\u0435 \u0438\u043c\u0435\u0435\u0442\u0441\u044f \u043d\u0438\u0447\u0435\u0433\u043e \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0433\u043e, \u0447\u0442\u043e \u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043b\u043e \u0431\u044b \u043e\u0442\u043c\u0435\u0442\u0438\u0442\u044c, \u0443\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u0439\u0442\u0435 \u0443\u043c \u043d\u0430 \u043f\u043e\u0434\u044a\u0435\u043c\u0435 \u0438 \u043f\u0430\u0434\u0435\u043d\u0438\u0438 \u0436\u0438\u0432\u043e\u0442\u0430. \u041f\u0440\u0438 \u0434\u0440\u0435\u043c\u043e\u0442\u0435 \u043e\u0442\u043c\u0435\u0447\u0430\u0439\u0442\u0435: \u00ab\u0434\u0440\u0435\u043c\u043e\u0442\u0430\u00bb, \u043f\u0440\u0438