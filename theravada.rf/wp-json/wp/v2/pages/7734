u0442\u044c\u044f \u043d\u0430\u0438\u0432\u044b\u0441\u0448\u0438\u043c \u043f\u0440\u0438\u043e\u0440\u0438\u0442\u0435\u0442\u043e\u043c. \u041f\u0440\u0438\u0432\u0435\u0434\u044f \u0432\u0441\u0435 \u0434\u0440\u0443\u0433\u0438\u0435 \u0441\u0432\u043e\u0438 \u0436\u0435\u043b\u0430\u043d\u0438\u044f \u043a \u0434\u0438\u0430\u043b\u043e\u0433\u0443 \u0441 \u0436\u0435\u043b\u0430\u043d\u0438\u0435\u043c \u0441\u0447\u0430\u0441\u0442\u044c\u044f, \u043e\u043d \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043b \u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u0435 \u0441\u0442\u0440\u0430\u0442\u0435\u0433\u0438\u0438, \u043f\u043e\u043a\u0430 \u043d\u0435 \u043d\u0430\u0448\u0451\u043b \u0435\u0434\u0438\u043d\u0441\u0442\u0432\u0435\u043d\u043d\u0443\u044e, \u043f\u043e\u043c\u043e\u0433\u0430\u044e\u0449\u0443\u044e \u0434\u043e\u0431\u0438\u0442\u044c\u0441\u044f \u043a\u043e\u043d\u0435\u0447\u043d\u043e\u0439 \u0446\u0435\u043b\u0438. \u042d\u0442\u0430 \u0441\u0442\u0440\u0430\u0442\u0435\u0433\u0438\u044f \u0441\u0442\u0430\u043b\u0430 \u0435\u0433\u043e \u043e\u0441\u043d\u043e\u0432\u043d\u044b\u043c \u043d\u0430\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u0438\u0435\u043c, \u2014 \u043d\u0430\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u0438\u0435\u043c \u043e \u0447\u0435\u0442\u044b\u0440\u0435\u0445 \u0431\u043b\u0430\u0433\u043e\u0440\u043e\u0434\u043d\u044b\u0445 \u0438\u0441\u0442\u0438\u043d\u0430\u0445.<\/span><\/p>\n<p><span style=\"font-family: arial, helvetica, sans-serif; font-size: 16px;\">\u0411\u043e\u043b\u044c\u0448\u0438\u043d\u0441\u0442\u0432\u043e \u0438\u0437 \u043d\u0430\u0441, \u0433\u043b\u044f\u0434\u044f \u043d\u0430 \u0447\u0435\u0442\u044b\u0440\u0435 \u0431\u043b\u0430\u0433\u043e\u0440\u043e\u0434\u043d\u044b\u0435 \u0438\u0441\u0442\u0438\u043d\u044b, \u043d\u0435 \u043f\u043e\u043d\u0438\u043c\u0430\u0435\u0442, \u0447\u0442\u043e \u0432\u0441\u0435 \u043e\u043d\u0438 \u043e\u0442\u043d\u043e\u0441\u044f\u0442\u0441\u044f \u043a \u0436\u0435\u043b\u0430\u043d\u0438\u044e. \u041d\u0430\u0441 \u0443\u0447\u0438\u043b\u0438, \u0447\u0442\u043e \u0411\u0443\u0434\u0434\u0430 \u043e\u0441\u0442\u0430\u0432\u0438\u043b \u0436\u0435\u043b\u0430\u043d\u0438\u044e \u0442\u043e\u043b\u044c\u043a\u043e \u043e\u0434\u043d\u0443 \u0440\u043e\u043b\u044c \u2014 \u043e\u043d\u043e \u043f\u0440\u0438\u0447\u0438\u043d\u0430 \u0441\u0442\u0440\u0430\u0434\u0430\u043d\u0438\u044f. \u041e\u043d \u0433\u043e\u0432\u043e\u0440\u0438\u043b, \u0447\u0442\u043e \u043f\u0440\u0438\u0447\u0438\u043d\u0430 \u0441\u0442\u0440\u0430\u0434\u0430\u043d\u0438\u044f \u0434\u043e\u043b\u0436\u043d\u0430 \u0431\u044b\u0442\u044c \u0443\u0441\u0442\u0440\u0430\u043d\u0435\u043d\u0430, \u0438 \u044d\u0442\u043e \u0437\u0432\u0443\u0447\u0438\u0442, \u043a\u0430\u043a \u0431\u0443\u0434\u0442\u043e \u043e\u043d \u043e\u0442\u0440\u0438\u0446\u0430\u0435\u0442 \u043b\u044e\u0431\u0443\u044e \u043f\u043e\u043b\u043e\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u0443\u044e \u0440\u043e\u043b\u044c \u0436\u0435\u043b\u0430\u043d\u0438\u044f \u0438 \u0435\u0433\u043e \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u043a\u0442\u0438\u0432\u043d\u044b\u0445 \u0441\u043f\u0443\u0442\u043d\u0438\u043a\u043e\u0432: \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u0442\u0432\u0430, \u0432\u043e\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u0438 \u043d\u0430\u0434\u0435\u0436\u0434\u044b. \u0422\u0430\u043a\u043e\u0435 \u043c\u043d\u0435\u043d\u0438\u0435, \u043e\u0434\u043d\u0430\u043a\u043e, \u0443\u043f\u0443\u0441\u043a\u0430\u0435\u0442 \u0438\u0437 \u0432\u0438\u0434\u0443 \u0434\u0432\u0430 \u0432\u0430\u0436\u043d\u044b\u0445 \u043c\u043e\u043c\u0435\u043d\u0442\u0430. \u041f\u0435\u0440\u0432\u044b\u0439, \u2014 \u0447\u0442\u043e \u0432\u0441\u0435 \u0447\u0435\u0442\u044b\u0440\u0435 \u0438\u0441\u0442\u0438\u043d\u044b \u0438\u0437\u043b\u0430\u0433\u0430\u044e\u0442 \u043e\u0441\u043d\u043e\u0432\u044b \u0434\