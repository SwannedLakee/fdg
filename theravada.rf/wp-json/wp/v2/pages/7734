43b\u0438 \u0432\u044b \u043a\u043e\u0433\u0434\u0430-\u043d\u0438\u0431\u0443\u0434\u044c \u043e\u0448\u0438\u0431\u043a\u0438? \u0415\u0441\u043b\u0438 \u0434\u0430, \u0447\u0442\u043e \u0431\u044b\u043b\u043e \u0441\u0430\u043c\u043e\u0439 \u0431\u043e\u043b\u044c\u0448\u043e\u0439 \u043e\u0448\u0438\u0431\u043a\u043e\u0439, \u043a\u043e\u0442\u043e\u0440\u0443\u044e \u0432\u044b \u043a\u043e\u0433\u0434\u0430-\u043b\u0438\u0431\u043e \u0441\u043e\u0432\u0435\u0440\u0448\u0430\u043b\u0438?\u00bb \u0422\u0435, \u043a\u0442\u043e \u043d\u0435 \u0431\u044b\u043b \u0443\u0441\u043f\u0435\u0448\u0435\u043d \u0432 \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u0438, \u043d\u0435\u0438\u0437\u043c\u0435\u043d\u043d\u043e \u043e\u0442\u0432\u0435\u0447\u0430\u043b\u0438, \u0447\u0442\u043e \u043e\u043d\u0438 \u0440\u0435\u0434\u043a\u043e \u043e\u0448\u0438\u0431\u0430\u044e\u0442\u0441\u044f \u0438\u043b\u0438 \u0432\u043e\u0437\u043b\u0430\u0433\u0430\u043b\u0438 \u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0441\u0442\u044c \u0437\u0430 \u043e\u0448\u0438\u0431\u043a\u0438 \u043d\u0430 \u0444\u0430\u043a\u0442\u043e\u0440\u044b, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0438\u043c \u043d\u0435\u043f\u043e\u0434\u0432\u043b\u0430\u0441\u0442\u043d\u044b. \u0422\u0435, \u043a\u0442\u043e \u043f\u0440\u0435\u0443\u0441\u043f\u0435\u043b \u0432 \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u0438, \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u043f\u0440\u0438\u0437\u043d\u0430\u0432\u0430\u043b\u0438\u0441\u044c \u0432\u043e \u043c\u043d\u043e\u0436\u0435\u0441\u0442\u0432\u0435 \u043e\u0448\u0438\u0431\u043e\u043a, \u043d\u043e \u0438 \u0432\u044b\u0440\u0430\u0436\u0430\u043b\u0438 \u043d\u0430\u043c\u0435\u0440\u0435\u043d\u0438\u0435 \u043d\u0435 \u043f\u043e\u0432\u0442\u043e\u0440\u044f\u0442\u044c \u044d\u0442\u0438 \u043e\u0448\u0438\u0431\u043a\u0438 \u0432 \u0431\u0443\u0434\u0443\u0449\u0435\u043c.<\/span><\/p>\n<p><span style=\"fo