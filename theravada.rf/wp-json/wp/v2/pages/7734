n style=\"font-family: arial, helvetica, sans-serif; font-size: 16px;\">\u041a \u0441\u0447\u0430\u0441\u0442\u044c\u044e, \u043c\u044b, \u043b\u044e\u0434\u0438, \u0438\u043c\u0435\u0435\u043c \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044c \u043e\u043a\u0443\u043b\u044c\u0442\u0443\u0440\u0438\u0442\u044c \u0441\u0432\u043e\u0438 \u043f\u0440\u0438\u0432\u044b\u0447\u043a\u0438 \u043f\u0438\u0442\u0430\u043d\u0438\u044f, \u043d\u0430\u0443\u0447\u0430\u044f\u0441\u044c \u043e\u0442\u043b\u0443\u0447\u0430\u0442\u044c \u0441\u0435\u0431\u044f \u043e\u0442 \u043d\u0435\u0437\u0434\u043e\u0440\u043e\u0432\u043e\u0439 \u043f\u0438\u0449\u0438 \u043e\u0431\u0440\u0430\u0437\u043e\u0432, \u0437\u0432\u0443\u043a\u043e\u0432, \u0437\u0430\u043f\u0430\u0445\u043e\u0432 \u0438 \u0442.\u0434., \u0438 \u043d\u0430\u0445\u043e\u0434\u0438\u0442\u044c \u0437\u0434\u043e\u0440\u043e\u0432\u0443\u044e \u043f\u0438\u0449\u0443 \u0432\u043d\u0443\u0442\u0440\u0438 \u0441\u0435\u0431\u044f. \u041a\u043e\u0433\u0434\u0430 \u043c\u044b \u0443\u0447\u0438\u043c\u0441\u044f \u0446\u0435\u043d\u0438\u0442\u044c \u0440\u0430\u0434\u043e\u0441\u0442\u044c \u0449\u0435\u0434\u0440\u043e\u0441\u0442\u0438, \u0447\u0435\u0441\u0442\u043d\u043e\u0441\u0442\u0438, \u0441\u043e\u0441\u0442\u0440\u0430\u0434\u0430\u043d\u0438\u044f \u0438 \u0434\u043e\u0432\u0435\u0440\u0438\u044f, \u043c\u044b \u0432\u0438\u0434\u0438\u043c, \u0447\u0442\u043e \u044d\u0442\u0430 \u0440\u0430\u0434\u043e\u0441\u0442\u044c \u043d\u0430\u0441\u044b\u0449\u0430\u0435\u0442 \u0441\u0438\u043b\u044c\u043d\u0435\u0435, \u0447\u0435\u043c \u043f\u0440\u043e\u0441\u0442\u043e\u0435 \u0443\u0434\u043e\u0432\u043e\u043b\u044c\u0441\u0442\u0432\u0438\u0435 \u043f\u0440\u0438\u0441\u0432\u043e\u0435\u043d\u0438\u044f \u0432\u0441\u0435\u0433\u043e, \u0434\u043e \u0447\u0435\u0433\u043e \u043c\u043e\u0436\u043d\u043e \u0434\u043e\u0442\u044f\u043d\u0443\u0442\u044c\u0441\u044f. \u041c\u044b \u043f\u043e\u0