43e\u0441\u0442\u044c \u043d\u0430 \u043e\u0449\u0443\u0449\u0435\u043d\u0438\u0438. \u041f\u043e\u043a\u0430 \u0432\u044b \u043e\u0441\u0442\u0430\u0435\u0442\u0435\u0441\u044c \u043d\u0430 \u044d\u0442\u043e\u043c \u043f\u0443\u0442\u0438, \u0432\u044b \u043d\u0435 \u0432\u0441\u0442\u0443\u043f\u0438\u0442\u0435 \u0432 \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u0435 \u0447\u0438\u0441\u0442\u043e\u0439 \u0441\u043e\u0441\u0440\u0435\u0434\u043e\u0442\u043e\u0447\u0435\u043d\u043d\u043e\u0441\u0442\u0438. \u041d\u043e \u0435\u0441\u043b\u0438 \u0432\u044b \u043e\u043f\u0438\u0440\u0430\u0435\u0442\u0435\u0441\u044c \u043f\u043e\u043b\u043d\u043e\u0441\u0442\u044c\u044e \u043d\u0430 \u043e\u0434\u043d\u0443 \u043d\u043e\u0433\u0443, \u043d\u0430 \u0441\u043e\u0441\u0440\u0435\u0434\u043e\u0442\u043e\u0447\u0435\u043d\u043d\u043e\u0441\u0442\u044c, \u0442\u043e\u0433\u0434\u0430 \u0432\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u0432\u0441\u0442\u0443\u043f\u0438\u0442\u044c \u043d\u0430 \u043f\u0443\u0442\u044c\u00a0<i>\u0441\u0430\u043c\u0430\u0442\u0445\u0430<\/i>, \u0441\u043c\u043e\u0436\u0435\u0442\u0435 \u0443\u0432\u0438\u0434\u0435\u0442\u044c \u0446\u0432\u0435\u0442\u0430, \u043e\u0431\u0440\u0430\u0437\u044b \u0438 \u0442. \u043f. \u041d\u043e \u0442\u0443\u0442 \u0432\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u043e\u0442\u0432\u043b\u0435\u0447\u044c\u0441\u044f \u043e\u0442 \u0446\u0435\u043b\u0438. \u0411\u0435\u0434\u0430 \u0432 \u0442\u043e\u043c, \u0447\u0442\u043e \u0442\u0435, \u043a\u0442\u043e \u0437\u0430\u043d\u044f\u0442\u044b\u00a0<i>\u0441\u0430\u043c\u0430\u0442\u0445\u0430<\/i>, \u043c\u043e\u0433\u0443\u0442 \u043f\u043e\u0447\u0443\u0432\u0441\u0442\u0432\u043e\u0432\u0430\u0442\u044c, \u0447\u0442\u043e \u043e\u043d\u0438 \u0447\u0435\u0433\u043e-\u0442\u043e \u0434\u043e\u0441\u0442\u0438\u0433\u043b\u0438, \u0442\u043e\u0433\u0434\u0430 \u043a\u0430\u043a \u0444\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438 \u0438\