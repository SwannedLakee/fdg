0446\u0438\u044f\u043c, \u043d\u043e \u0432\u0441\u043b\u0435\u0434\u0441\u0442\u0432\u0438\u0435 \u0431\u043e\u043b\u044c\u0448\u0435\u0433\u043e \u043e\u0441\u043e\u0437\u043d\u0430\u043d\u0438\u044f \u0442\u043e\u0433\u043e, \u0447\u0442\u043e \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u0438\u0442 \u043f\u0440\u0438 \u0431\u0435\u0437\u043d\u0440\u0430\u0432\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u043c \u043f\u043e\u0432\u0435\u0434\u0435\u043d\u0438\u0438. \u041f\u0435\u0447\u0430\u043b\u044c, \u043d\u0435\u0441\u0447\u0430\u0441\u0442\u044c\u0435, \u0431\u043e\u043b\u044c \u0438 \u0433\u043e\u0440\u0435 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u044e\u0442 \u0434\u0432\u043e\u0439\u0441\u0442\u0432\u0435\u043d\u043d\u0443\u044e \u0444\u043e\u0440\u043c\u0443 \u2013 \u0444\u0438\u0437\u0438\u0447\u0435\u0441\u043a\u0443\u044e \u0438 \u0434\u0443\u0448\u0435\u0432\u043d\u0443\u044e. \u0424\u0438\u0437\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u043d\u0435\u0441\u0447\u0430\u0441\u0442\u044c\u0435 \u0438 \u0431\u043e\u043b\u044c \u0432\u043e\u0437\u043d\u0438\u043a\u0430\u044e\u0442, \u043a\u043e\u0433\u0434\u0430 \u0442\u0435\u043b\u043e \u043f\u043e\u0434\u0432\u0435\u0440\u0436\u0435\u043d\u043e \u0431\u043e\u043b\u0435\u0437\u043d\u0438, \u043a\u043e\u0433\u0434\u0430 \u043e\u043d\u043e \u043d\u0435 \u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0438\u0440\u0443\u0435\u0442 \u0434\u043e\u043b\u0436\u043d\u044b\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c; \u0433\u043e\u0440\u0435 \u0438 \u043f\u0435\u0447\u0430\u043b\u044c \u043f\u043e\u044f\u0432\u043b\u044f\u044e\u0442\u0441\u044f \u043f\u0440\u0438 \u0440\u0430\u0441\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u0430\u0445 \u0443\u043c\u0430. \u042d\u0442\u0430 \u0444\u043e\u0440\u043c\u0430 \u043c\u0435\u0434\u0438\u0442\u0430\u0446\u0438\u0438 \u043f\u043e\u043c\u043e\u0433\u0430\u0435\u0442 \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e\u0439 \u0444\u0443\u043d\u043a\u0446\u04