0442\u0430\u043d\u043e\u0432\u043a\u0430 \u0434\u043b\u044f \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u0438 \u0443\u0447\u0435\u043d\u0438\u044f. \u042d\u0442\u0430 \u043e\u0431\u0441\u0442\u0430\u043d\u043e\u0432\u043a\u0430 \u0431\u044b\u043b\u0430 \u0437\u0430\u0434\u0443\u043c\u0430\u043d\u0430 \u043a\u0430\u043a \u043f\u043e\u0434\u0430\u0440\u043e\u043a \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0442\u0435\u043c, \u043a\u0442\u043e \u0432\u043e\u0439\u0434\u0435\u0442 \u0432 \u043e\u0431\u0449\u0438\u043d\u0443, \u043d\u043e \u0438 \u0441\u0432\u044f\u0437\u0430\u043d\u043d\u044b\u043c \u0441 \u043e\u0431\u0449\u0438\u043d\u043e\u0439 \u043c\u0438\u0440\u044f\u043d\u0430\u043c, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043c\u043e\u0433\u0443\u0442 \u0432\u043e\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u044d\u0442\u043e\u0439 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044c\u044e \u0440\u0430\u0437\u0432\u0438\u0442\u044c \u0441\u0432\u043e\u044e \u0449\u0435\u0434\u0440\u043e\u0441\u0442\u044c, \u043c\u043e\u0440\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u044c, \u0438 \u043e\u0441\u043e\u0437\u043d\u0430\u043d\u043d\u043e\u0441\u0442\u044c. \u0421\u043e\u043e\u0431\u0449\u0435\u0441\u0442\u0432\u043e \u0434\u0440\u0443\u0433\u0438\u0445 \u043b\u044e\u0434\u0435\u0439, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0441\u043b\u0435\u0434\u0443\u044e\u0442 \u0442\u043e\u0447\u043d\u043e\u043c\u0443 \u043a\u043e\u0434\u0435\u043a\u0441\u0443 \u0434\u0438\u0441\u0446\u0438\u043f\u043b\u0438\u043d\u044b, \u0437\u0430\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u0431\u044b\u0442\u044c \u0431\u043e\u043b\u0435\u0435 \u0442\u043e\u0447\u043d\u044b\u043c \u0438 \u0434\u0438\u0441\u0446\u0438\u043f\u043b\u0438\u043d\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u043c. \u0425\u043e\u0442\u044f \u043e\u0441\u043d\u043e\u0432\u043d\u0430\u044f \u0442\u0435\u043c\u0430 \u044d\u0442\u043e\u0439 \u043a\u