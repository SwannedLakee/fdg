3a. \u041a\u0438\u0441\u0430\u0432\u0430\u043a\u0447\u0430, \u043f\u043e\u043f\u0440\u043e\u0441\u0438\u0432 \u0440\u0430\u0437\u0440\u0435\u0448\u0435\u043d\u0438\u044f \u0443 \u0411\u043e\u0434\u0445\u0438\u0441\u0430\u0442\u0442\u0432\u044b, \u043f\u043e\u0441\u0435\u043b\u0438\u043b\u0441\u044f \u0432 \u043f\u0430\u0440\u043a\u0435 \u0440\u044f\u0434\u043e\u043c \u0441 \u0433\u043b\u0430\u0432\u043d\u043e\u043a\u043e\u043c\u0430\u043d\u0434\u0443\u044e\u0449\u0438\u043c, \u0432 \u0433\u043e\u0440\u043e\u0434\u0435 \u041a\u0443\u043c\u0431\u0445\u0430\u0432\u0430\u0442\u0438, \u0432 \u043f\u0440\u043e\u0432\u0438\u043d\u0446\u0438\u0438 \u0446\u0430\u0440\u044f \u0414\u0430\u043c\u0430\u043a\u0438. \u041d\u0430\u0440\u0430\u0434\u0430 \u043f\u043e\u0441\u0435\u043b\u0438\u043b\u0441\u044f \u0432 \u0446\u0435\u043d\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u0439 \u043f\u0440\u043e\u0432\u0438\u043d\u0446\u0438\u0438 \u0432 \u0433\u043e\u0440\u043d\u043e\u0439 \u0446\u0435\u043f\u0438 \u0410\u0440\u0430\u043d\u044c\u044f\u0440\u0430. \u0410\u043d\u0443\u0441\u0438\u0441\u0441\u0430 \u0436\u0435 \u043e\u0441\u0442\u0430\u043b\u0441\u044f \u0441 \u0411\u043e\u0434\u0445\u0438\u0441\u0430\u0442\u0442\u043e\u0439. \u0412 \u044d\u0442\u043e \u0432\u0440\u0435\u043c\u044f \u0446\u0430\u0440\u044c \u0414\u0430\u0440\u0430\u0434\u0430\u043a\u0438 \u0441\u043c\u0435\u0441\u0442\u0438\u043b \u0441\u0432\u043e\u044e \u043a\u0443\u0440\u0442\u0438\u0437\u0430\u043d\u043a\u0443, \u043a\u043e\u0442\u043e\u0440\u0443\u044e \u0434\u043e \u044d\u0442\u043e\u0433\u043e \u043e\u0447\u0435\u043d\u044c \u043f\u043e\u0447\u0438\u0442\u0430\u043b, \u0438 \u0442\u0430, \u0433\u0440\u0443\u0441\u0442\u043d\u043e \u0431\u0440\u043e\u0434\u044f \u043f\u043e \u043f\u0430\u0440\u043a\u0443, \u0443\u0432\u0438\u0434\u0435\u043b\u0430 \u0430\u0441\u043a\u0435\u0442\u0430 \u041a\u0438\u0441\u0430\u0432\u0430\u043a\u0447\u0443 \u0438 \u043f\u043e\u0434\u0443\u043c\u0430\u043b\u0430: &#171;\u0423 \