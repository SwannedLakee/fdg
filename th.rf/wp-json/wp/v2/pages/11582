043b\u043e\u0433\u043e\u0432\u043e\u0440\u044f\u0449\u0438\u0435 \u043c\u043e\u043d\u0430\u0445\u0438 \u043f\u0440\u043e\u0445\u043e\u0434\u044f\u0442 \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u0435 \u0432 \u0442\u0430\u0439\u0441\u043a\u043e\u0439 \u043b\u0435\u0441\u043d\u043e\u0439 \u0442\u0440\u0430\u0434\u0438\u0446\u0438\u0438. \u042d\u0442\u043e\u0442 \u043e\u0431\u0440\u0430\u0437 \u0436\u0438\u0437\u043d\u0438 \u043f\u0440\u0435\u0434\u0443\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u0435\u0442 \u0437\u043d\u0430\u0447\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0435 \u0444\u0438\u0437\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u043d\u0430\u0433\u0440\u0443\u0437\u043a\u0438 \u0438 \u0440\u0435\u0433\u0443\u043b\u044f\u0440\u043d\u044b\u0435 \u0433\u0440\u0443\u043f\u043f\u043e\u0432\u044b\u0435 \u0441\u043e\u0431\u0440\u0430\u043d\u0438\u044f, \u0430 \u0442\u0430\u043a\u0436\u0435 \u0441\u0442\u0440\u043e\u0433\u0438\u0439 \u0438 \u0442\u043e\u0447\u043d\u044b\u0439 \u0444\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u044b\u0439 \u044d\u0442\u0438\u043a\u0435\u0442. \u041a\u043e\u0433\u0434\u0430 \u0411\u0445\u0430\u043d\u0442\u0435 \u043f\u043e\u0441\u0435\u0442\u0438\u043b \u0412\u0430\u0442 \u041f\u0430 \u041d\u0430\u043d\u0430\u0447\u0430\u0442, \u043e\u043d \u043f\u043e\u0447\u0443\u0432\u0441\u0442\u0432\u043e\u0432\u0430\u043b \u0441\u0435\u0431\u044f \u043d\u0435 \u0432 \u0441\u0432\u043e\u0435\u0439 \u0442\u0430\u0440\u0435\u043b\u043a\u0435, \u043f\u043e\u0441\u043a\u043e\u043b\u044c\u043a\u0443 \u0431\u043e\u043b\u044c\u0448\u0435 \u0432\u0441\u0435\u0433\u043e \u043d\u0430 \u0441\u0432\u0435\u0442\u0435 \u043e\u043d \u0446\u0435\u043d\u0438\u043b \u0441\u0432\u043e\u044e \u043d\u0435\u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u044c, \u0438 \u0436\u0438\u0437\u043d\u044c \u0432 \u0436\u0451\u0441\u0442\u043a\u043e \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u043e\u0432\u0430\u043d\u043d\u043e\u0439 \u043e\u0431\u0449\u0438\u043