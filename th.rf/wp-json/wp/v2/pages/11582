43e\u0431\u0440\u0430\u0437 \u0436\u0438\u0437\u043d\u0438 \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u0441\u0443\u0440\u043e\u0432\u044b\u043c \u0434\u043b\u044f \u0441\u0435\u0431\u044f. \u0412 \u0442\u0435\u0447\u0435\u043d\u0438\u0435 \u043d\u0430\u0448\u0435\u0439 \u043f\u0435\u0440\u0432\u043e\u0439 \u0432\u0441\u0442\u0440\u0435\u0447\u0438 \u044f \u0442\u0430\u043a\u0436\u0435 \u0431\u044b\u0441\u0442\u0440\u043e \u043f\u043e\u043d\u044f\u043b, \u043d\u0430\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u043e\u0441\u0442\u0440\u0430 \u0438 \u0431\u043b\u0435\u0441\u0442\u044f\u0449\u0430 \u0431\u044b\u043b\u0430 \u0435\u0433\u043e \u043f\u0430\u043c\u044f\u0442\u044c! \u0414\u043b\u044f \u043c\u0435\u043d\u044f \u0431\u044b\u043b\u043e \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0438\u043c \u0438\u0441\u043f\u044b\u0442\u0430\u043d\u0438\u0435\u043c \u043d\u0435 \u0441\u043c\u0443\u0449\u0430\u0442\u044c \u0441\u0435\u0431\u044f \u0441\u043b\u0430\u0431\u043e\u0439 \u043f\u0430\u043c\u044f\u0442\u044c\u044e, \u0438 \u044f \u043f\u043e\u043d\u044f\u043b, \u0447\u0442\u043e \u0438\u0441\u0442\u0438\u043d\u043d\u044b\u0439 \u0432\u043e\u0437\u0440\u0430\u0441\u0442 \u0434\u043e\u043b\u0436\u0435\u043d \u0438\u0437\u043c\u0435\u0440\u044f\u0442\u044c\u0441\u044f \u043d\u0435 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043e\u043c \u0442\u0435\u043b\u0430, \u0430 \u0442\u0435\u043c, \u043a\u0430\u043a \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u043f\u0438\u0442\u0430\u0435\u0442 \u0441\u0432\u043e\u0439 \u0440\u0430\u0437\u0443\u043c: \u043d\u0435 \u043f\u043e\u0442\u0430\u043a\u0430\u044f \u043b\u0435\u043d\u0438, \u0430 \u0432\u044b\u0432\u043e\u0434\u044f \u0441\u0435\u0431\u044f \u0437\u0430 \u043f\u0440\u0435\u0434\u0435\u043b\u044b \u0437\u043e\u043d\u044b \u043a\u043e\u043c\u0444\u043e\u0440\u0442\u0430!<\/span><\/p>\n<p><span style=\"font-size: 20px; font-family: arial, helvetica, sans-serif;\">\u041d\u0430\u0448\u0438 \u0434\u0430\u043b\u044c\u043d\u0435\u0