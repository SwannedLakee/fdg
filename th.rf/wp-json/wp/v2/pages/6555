43e \u0441\u0432\u0435\u0440\u0445\u044a\u0435\u0441\u0442\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0435. \u041a\u0440\u043e\u043c\u0435 \u0442\u043e\u0433\u043e, \u041d\u0438\u0440\u0432\u0430\u043d\u0430 \u0434\u043e\u0441\u0442\u0438\u0433\u0430\u0435\u0442\u0441\u044f \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u043f\u043e\u0441\u043b\u0435 \u0441\u043c\u0435\u0440\u0442\u0438. \u042d\u0442\u043e\u0442 \u043c\u043e\u043c\u0435\u043d\u0442 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e \u043f\u043e\u043d\u0438\u043c\u0430\u0442\u044c. \u041d\u0438\u0440\u0432\u0430\u043d\u0430 \u0434\u043e\u0441\u0442\u0438\u0433\u0430\u0435\u0442\u0441\u044f \u043a\u0430\u0436\u0434\u044b\u0439 \u0440\u0430\u0437, \u043a\u043e\u0433\u0434\u0430 \u0443\u043c \u0441\u0442\u0430\u043d\u043e\u0432\u0438\u0442\u0441\u044f \u0441\u0432\u043e\u0431\u043e\u0434\u043d\u044b\u043c \u043e\u0442 \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f. \u0421\u0432\u043e\u0431\u043e\u0434\u0430 \u043e\u0442 \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0432 \u043b\u044e\u0431\u043e\u0439 \u043c\u043e\u043c\u0435\u043d\u0442, \u043a\u043e\u0433\u0434\u0430 \u043e\u043d\u0430 \u0435\u0441\u0442\u044c \u2013 \u044d\u0442\u043e \u041d\u0438\u0440\u0432\u0430\u043d\u0430. \u041f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u043e\u0435 \u043f\u0440\u0435\u043a\u0440\u0430\u0449\u0435\u043d\u0438\u0435 \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u2013 \u044d\u0442\u043e \u043e\u043a\u043e\u043d\u0447\u0430\u0442\u0435\u043b\u044c\u043d\u0430\u044f \u041d\u0438\u0440\u0432\u0430\u043d\u0430. \u0412\u0440\u0435\u043c\u0435\u043d\u043d\u043e\u0435 \u043f\u0440\u0435\u043a\u0440\u0430\u0449\u0435\u043d\u0438\u0435 \u2013 \u044d\u0442\u043e \u043b\u0438\u0448\u044c \u043a\u0440\u0430\u0442\u043a\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u0430\u044f \u041d\u0438\u0440\u0432\u0430\u043d\u0430, \u0438 \u043c\u044b \u044d\u0442\u043e \u0443\u0436\u0435 \u043e\u0431\u0441\u0443\u0436\u0434\u0430\u043b\u0438. \u041f\u0435\u0440\u0435\u0436\u0438\u0432\u0430\u043d\u0438\u0435 \u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e\u0439 \u041d\u0438\u0440\u0432\u0430\u043d\u044b \u0441\u043b\u0443\u0436\u0438\u0442 \u0441\u0442\u0438\u043c\u0443\u043b\u043e\u043c \u0434\u043b\u044f \u0442\u043e\u0433\u043e, \u0447\u0442\u043e\u0431\u044b \u0438\u0434\u0442\u0438 \u0434\u0430\u043b\u044c\u0448\u0435, \u0447\u0442\u043e\u0431\u044b \u0434\u043e\u0441\u0442\u0438\u0447\u044c \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u043e\u0439 \u041d\u0438\u0440\u0432\u0430\u043d\u044b, \u043f\u043e\u043b\u043d\u043e\u0439 \u041d\u0438\u0440\u0432\u0430\u043d\u044b, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u0434\u0435\u043b\u0430\u0435\u0442 \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430 \u0430\u0440\u0430\u0445\u0430\u043d\u0442\u043e\u043c. \u042d\u0442\u043e \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u0435 \u0432\u043e\u0437\u043d\u0438\u043a\u0430\u0435\u0442 \u043e\u0442 \u043e\u0441\u043e\u0437\u043d\u0430\u043d\u0438\u044f \u0442\u043e\u0433\u043e, \u0447\u0442\u043e \u0441\u0430\u043d\u043a\u0445\u0430\u0440\u044b, \u0442\u043e \u0435\u0441\u0442\u044c \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u0438 \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u044b \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u044f\u0432\u043b\u044f\u044e\u0442\u0441\u044f \u043d\u0435\u0441\u0447\u0430\u0441\u0442\u044c\u0435\u043c, \u0432 \u0442\u043e \u0432\u0440\u0435\u043c\u044f \u043a\u0430\u043a \u041d\u0438\u0440\u0432\u0430\u043d\u0430, \u0442\u043e \u0435\u0441\u0442\u044c \u0441\u0432\u043e\u0431\u043e\u0434\u0430 \u043e\u0442 \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0443\u043c\u0438\u0440\u043e\u0442\u0432\u043e\u0440\u0435\u043d\u0438\u0435\u043c, \u0431\u043b\u0430\u0436\u0435\u043d\u0441\u0442\u0432\u043e\u043c. \u0426\u0435\u043b\u044c\u044e \u043a\u0430\u0436\u0434\u043e\u0433\u043e \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430 \u0432 \u0436\u0438\u0437\u043d\u0438 \u0434\u043e\u043b\u0436\u0435\u043d \u0431\u044b\u0442\u044c \u043f\u0443\u0442\u044c, \u0432\u0435\u0434\u0443\u0449\u0438\u0439 \u043a \u043f\u043e\u043b\u043d\u043e\u0439 \u041d\u0438\u0440\u0432\u0430\u043d\u0435. <\/span><br \/>\n<span style=\"font-size: 14pt; font-family: arial, helvetica, sans-serif;\">\u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043e\u0442\u0432\u0435\u0442 \u043d\u0430 \u0432\u043e\u043f\u0440\u043e\u0441: \u00ab\u0417\u0430\u0447\u0435\u043c \u043c\u044b \u0440\u043e\u0434\u0438\u043b\u0438\u0441\u044c?\u00bb &#8212; \u0437\u0430\u043a\u043b\u044e\u0447\u0430\u0435\u0442\u0441\u044f \u0432 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0435\u043c:<\/span><br \/>\n<span style=\"font-size: 14pt; font-family: arial, helvetica, sans-serif;\">\u041a\u043e\u043d\u0441\u0442\u0440\u0443\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u2013 \u044d\u0442\u043e \u0432\u0441\u0435\u043e\u0445\u0432\u0430\u0442\u044b\u0432\u0430\u044e\u0449\u0435\u0435 \u0441\u0442\u0440\u0430\u0434\u0430\u043d\u0438\u0435,<\/span><br \/>\n<span style=\"font-size: 14pt; font-family: arial, helvetica, sans-serif;\">\u041d\u0438\u0440\u0432\u0430\u043d\u0430 \u2013 \u044d\u0442\u043e \u0432\u044b\u0441\u0448\u0435\u0435 \u0431\u043b\u0430\u0436\u0435\u043d\u0441\u0442\u0432\u043e.<\/span><\/p>\n","protected":false},"excerpt":{"rendered":"<p>\u0417\u0430\u0447\u0435\u043c \u043c\u044b \u0440\u043e\u0434\u0438\u043b\u0438\u0441\u044c? \u0414\u043e\u0441\u0442. \u0411\u0443\u0434\u0434\u0430\u0434\u0430\u0441\u0430 \u0411\u0445\u0438\u043a\u043a\u0445\u0443 \u041f\u0435\u0440. \u0441 \u0430\u043d\u0433\u043b.: \u0426\u0432\u0435\u0442\u043a