u041c\u044b \u0442\u0435\u0440\u043f\u0435\u043b\u0438\u0432\u043e \u0441\u043b\u0443\u0448\u0430\u0435\u043c, \u0438 \u043c\u044b \u0441\u043f\u043e\u043a\u043e\u0439\u043d\u044b, \u043d\u043e \u0442\u0432\u0435\u0440\u0434\u044b, \u043a\u043e\u0433\u0434\u0430 \u043d\u0430\u0441 \u043f\u0440\u0435\u0440\u044b\u0432\u0430\u044e\u0442. \u041c\u044b \u0441\u0442\u0430\u0440\u0430\u0435\u043c\u0441\u044f \u0432\u0441\u0435\u0433\u0434\u0430 \u043f\u043e\u043c\u043d\u0438\u0442\u044c \u043e \u0436\u0435\u0441\u0442\u0430\u0445 \u0438 \u0442\u043e\u043d\u0435 \u0433\u043e\u043b\u043e\u0441\u0430. \u041c\u044b \u043f\u0440\u043e\u044f\u0432\u043b\u044f\u0435\u043c \u0443\u0432\u0430\u0436\u0435\u043d\u0438\u0435 \u043a \u0441\u043b\u043e\u0432\u0430\u043c \u0434\u0440\u0443\u0433\u043e\u0433\u043e \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430 \u0438 \u0438\u0441\u043a\u0440\u0435\u043d\u043d\u0435 \u043f\u044b\u0442\u0430\u0435\u043c\u0441\u044f \u0435\u0433\u043e \u043f\u043e\u043d\u044f\u0442\u044c. \u041c\u044b \u043d\u0435 \u0437\u0430\u043d\u0438\u043c\u0430\u0435\u043c \u043f\u043e\u0437\u0438\u0446\u0438\u0438: \u00ab\u044f \u043f\u0440\u0430\u0432, \u0430 \u0442\u044b \u043d\u0435\u0442\u00bb, \u00ab\u044f \u0445\u043e\u0440\u043e\u0448\u0438\u0439, \u0442\u044b \u043f\u043b\u043e\u0445\u043e\u0439\u00bb, \u00ab\u044f \u0443\u043c\u043d\u044b\u0439, \u0442\u044b \u0433\u043b\u0443\u043f\u044b\u0439\u00bb. \u041c\u044b \u0437\u043d\u0430\u0435\u043c, \u0447\u0442\u043e \u0432 \u0441\u0430\u043c\u044b\u0445 \u043f\u043b\u043e\u0434\u043e\u0442\u0432\u043e\u0440\u043d\u044b\u0445 \u0441\u043f\u043e\u0440\u0430\u0445, \u043d\u0435\u0442 \u043f\u043e\u0431\u0435\u0434\u0438\u0442\u0435\u043b\u0435\u0439 \u0438 \u043f\u0440\u043e\u0438\u0433\u0440\u0430\u0432\u0448\u0438\u0445, \u0438 \u0441\u0442\u0430\u0440\u0430\u0435\u043c\u0441\u044f \u0432\u0435\u0441\u0442\u0438 \u0440\u0430\u0437\u0433\u043e\u0432\u043e\u0440 \u0442\u0430\u043a, \u0431\u0443\u0434\u0442\u043