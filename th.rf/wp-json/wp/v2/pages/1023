442\u0432. \u041e\u043d\u0438 \u043f\u0440\u043e\u0441\u0442\u043e \u043f\u0440\u043e\u044f\u0432\u043b\u044f\u044e\u0442\u0441\u044f \u0435\u0441\u0442\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c. \u042f \u0441\u043e\u0432\u0435\u0442\u0443\u044e \u0439\u043e\u0433\u0438\u043d\u0430\u043c \u043d\u0435 \u0443\u0434\u0435\u043b\u044f\u0442\u044c \u043c\u043d\u043e\u0433\u043e \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u044f \u043f\u0440\u0438\u044f\u0442\u043d\u043e\u043c\u0443 \u0438\u043b\u0438 \u043d\u0435\u043f\u0440\u0438\u044f\u0442\u043d\u043e\u043c\u0443, \u0442\u0430\u043a \u043a\u0430\u043a, \u043f\u043e\u0441\u0442\u0443\u043f\u0430\u044f \u0442\u0430\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c, \u043c\u044b \u043d\u0430 \u0441\u0430\u043c\u043e\u043c \u0434\u0435\u043b\u0435 \u0443\u0432\u0435\u043a\u043e\u0432\u0435\u0447\u0438\u0432\u0430\u0435\u043c \u044d\u0442\u0438 \u0441\u043f\u0435\u0446\u0438\u0444\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u043f\u0440\u0438\u044f\u0442\u043d\u044b\u0435 \u0438\u043b\u0438 \u043d\u0435\u043f\u0440\u0438\u044f\u0442\u043d\u044b\u0435 \u0447\u0443\u0432\u0441\u0442\u0432\u0430. \u042f \u0445\u043e\u0442\u0435\u043b \u0431\u044b, \u0447\u0442\u043e\u0431\u044b \u0439\u043e\u0433\u0438\u043d\u044b \u0443\u0434\u0435\u043b\u044f\u043b\u0438 \u0431\u043e\u043b\u044c\u0448\u0435 \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u044f \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0443 \u0447\u0443\u0432\u0441\u0442\u0432\u043e\u0432\u0430\u043d\u0438\u044f, \u0447\u0435\u043c \u044d\u0442\u0438\u043c \u043f\u0440\u0438\u044f\u0442\u043d\u044b\u043c, \u043d\u0435\u043f\u0440\u0438\u044f\u0442\u043d\u044b\u043c \u0438\u043b\u0438 \u043d\u0435\u0439\u0442\u0440\u0430\u043b\u044c\u043d\u044b\u043c \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0430\u043c. \u041e\u0441\u043e\u0437\u043d\u0430\u0432\u0430\u043d\u0438\u0435 \u0447\u0443\u0432\u0441\u0442\u0432 \u0431\u04