43b \u043d\u0430\u0447\u0430\u0442\u044c \u0441 \u043f\u0435\u0440\u0432\u043e\u0439 \u043f\u0440\u043e\u043f\u043e\u0432\u0435\u0434\u0438 \u0411\u0443\u0434\u0434\u044b. \u041e\u043d\u0430 \u043f\u043e\u0434\u043d\u0438\u043c\u0430\u0435\u0442 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u044b\u0439 \u0432\u043e\u043f\u0440\u043e\u0441. \u042d\u0442\u0430 \u043f\u0440\u043e\u043f\u043e\u0432\u0435\u0434\u044c \u0431\u044b\u043b\u0430 \u0430\u0434\u0440\u0435\u0441\u043e\u0432\u0430\u043d\u0430 \u0433\u0440\u0443\u043f\u043f\u0435 \u0438\u0437 \u043f\u044f\u0442\u0438 \u043c\u043e\u043d\u0430\u0445\u043e\u0432, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0431\u044b\u043b\u0438, \u043d\u0430 \u0442\u043e \u0432\u0440\u0435\u043c\u044f, \u043d\u0435\u0431\u0443\u0434\u0434\u0438\u0441\u0442\u0441\u043a\u0438\u043c\u0438 \u0430\u0441\u043a\u0435\u0442\u0430\u043c\u0438. \u041e\u0434\u043d\u0430\u043a\u043e, \u0442\u0435\u043a\u0441\u0442 \u0441\u0441\u044b\u043b\u0430\u0435\u0442\u0441\u044f \u043d\u0430 \u043e\u0441\u043e\u0437\u043d\u0430\u043d\u043d\u043e\u0441\u0442\u044c \u0442\u0430\u043a, \u0431\u0443\u0434\u0442\u043e \u043f\u043e\u0434\u0440\u0430\u0437\u0443\u043c\u0435\u0432\u0430\u0435\u0442\u0441\u044f, \u0447\u0442\u043e \u0430\u0443\u0434\u0438\u0442\u043e\u0440\u0438\u044f \u0443\u0436\u0435 \u0437\u043d\u0430\u0435\u0442, \u0447\u0442\u043e \u044d\u0442\u043e \u0442\u0430\u043a\u043e\u0435. \u041c\u0430\u0445\u0430\u0441\u0438 \u0421\u0430\u044f\u0434\u043e \u0437\u0430\u043c\u0435\u0442\u0438\u043b \u044d\u0442\u043e, \u0438 \u043f\u0440\u0435\u0434\u043f\u043e\u043b\u043e\u0436\u0438\u043b, \u0447\u0442\u043e \u044d\u0442\u0430 \u043b\u0435\u043a\u0446\u0438\u044f \u0434\u043e\u043b\u0436\u043d\u0430 \u0431\u044b\u043b\u0430 \u0438\u0437\u043d\u0430\u0447\u0430\u043b\u044c\u043d\u043e \u0432\u0445\u043e\u0434\u0438\u0442\u044c \u0432 \u0431\u043e\u043b\u0435\u0435 \u0434\u0435\u0442\u0430\u043b\u044c\u043d\u043e\u0435 \u043e\u0431\u044a\u044f\u