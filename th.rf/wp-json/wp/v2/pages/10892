043a \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0436\u0435\u043b\u0430\u0435\u0442 \u0441\u0435\u0431\u0435 \u0431\u043b\u0430\u0433\u0430, \u0435\u043c\u0443 \u0431\u044b \u0441\u0442\u043e\u0438\u043b\u043e \u043f\u0440\u0438\u043d\u044f\u0442\u044c \u0442\u0430\u043a\u043e\u0435 (\u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0435\u043d\u0438\u0435). \u041f\u043e\u0447\u0435\u043c\u0443 \u0442\u0430\u043a? \u0418\u0437 \u043d\u0435\u043e\u0431\u043e\u0437\u0440\u0438\u043c\u043e\u0433\u043e \u043d\u0430\u0447\u0430\u043b\u0430 \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u044f\u0442 \u043f\u0435\u0440\u0435\u0440\u043e\u0436\u0434\u0435\u043d\u0438\u044f. \u041d\u0435 \u043d\u0430\u0439\u0442\u0438 \u043d\u0430\u0447\u0430\u043b\u0430 (\u0431\u043e\u043b\u0438 \u043e\u0442) \u0432\u0441\u0435\u043c \u044d\u0442\u0438\u043c \u0443\u0434\u0430\u0440\u0430\u043c \u043a\u043e\u043f\u044c\u044f\u043c\u0438, \u043c\u0435\u0447\u0430\u043c\u0438 \u0438 \u0442\u043e\u043f\u043e\u0440\u0430\u043c\u0438. \u0418 \u0435\u0441\u043b\u0438 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0431\u044b\u043b\u043e \u0431\u044b \u0442\u0430\u043a\u043e\u0435 (\u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0435\u043d\u0438\u0435), \u0433\u043e\u0432\u043e\u0440\u044e \u0432\u0430\u043c, \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u0435 \u0427\u0435\u0442\u044b\u0440\u0435\u0445 \u0411\u043b\u0430\u0433\u043e\u0440\u043e\u0434\u043d\u044b\u0445 \u0418\u0441\u0442\u0438\u043d \u043d\u0435 \u0441\u043e\u043f\u0440\u043e\u0432\u043e\u0436\u0434\u0430\u043b\u043e\u0441\u044c \u0431\u044b \u0431\u043e\u043b\u044c\u044e \u0438 \u043f\u0435\u0447\u0430\u043b\u044c\u044e. \u041d\u0430\u043e\u0431\u043e\u0440\u043e\u0442, \u0433\u043e\u0432\u043e\u0440\u044e \u0432\u0430\u043c, \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u0435 \u0427\u0435\u0442\u044b\u0440\u0435\u0445 \u0411\u043b\u0430\u0433\u043e\u0440\u043e\u043