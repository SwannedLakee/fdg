\/page"}],"author":[{"embeddable":true,"href":"http:\/\/xn--80aaaglc1fo1a.xn--p1ai\/wp-json\/wp\/v2\/users\/1"}],"replies":[{"embeddable":true,"href":"http:\/\/xn--80aaaglc1fo1a.xn--p1ai\/wp-json\/wp\/v2\/comments?post=11648"}],"version-history":[{"count":1,"href":"http:\/\/xn--80aaaglc1fo1a.xn--p1ai\/wp-json\/wp\/v2\/pages\/11648\/revisions"}],"predecessor-version":[{"id":11649,"href":"http:\/\/xn--80aaaglc1fo1a.xn--p1ai\/wp-json\/wp\/v2\/pages\/11648\/revisions\/11649"}],"up":[{"embeddable":true,"href":"http:\/\/xn--80aaaglc1fo1a.xn--p1ai\/wp-json\/wp\/v2\/pages\/28"}],"wp:attachment":[{"href":"http:\/\/xn--80aaaglc1fo1a.xn--p1ai\/wp-json\/wp\/v2\/media?parent=11648"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}