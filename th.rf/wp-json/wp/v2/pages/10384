u0435\u0448\u043d\u0438\u0435 \u0438 \u043d\u0430\u0440\u0443\u0436\u043d\u044b\u0435 \u043e\u0431\u0441\u0442\u043e\u044f\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u0434\u043e\u043b\u0436\u043d\u044b \u043e\u0441\u0442\u0430\u0432\u0430\u0442\u044c\u0441\u044f \u0441\u043f\u043e\u0441\u043e\u0431\u0441\u0442\u0432\u0443\u044e\u0449\u0438\u043c\u0438 \u044d\u0442\u043e\u0439 \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u0435 \u0443\u0433\u043b\u0443\u0431\u043b\u0435\u043d\u0438\u044f \u0432 \u043f\u0443\u0441\u0442\u043e\u0442\u0443. \u041d\u0430\u0448\u0430 \u0426\u0435\u043b\u044c \u2014 \u041f\u0443\u0441\u0442\u043e\u0442\u0430 \u2014 \u0421\u0443\u043d\u044c\u044f\u0442\u0430. \u041d\u0435\u0442 \u043d\u0438\u0447\u0435\u0433\u043e \u0432\u0430\u0436\u043d\u0435\u0435 \u0432 \u0436\u0438\u0437\u043d\u0438. \u042d\u0442\u043e \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u044f \u0436\u0438\u0437\u043d\u0438 \u0438 \u0441\u043c\u0435\u0440\u0442\u0438, \u0438 \u0432\u0441\u0451 \u0436\u0435 \u043d\u0435 \u0434\u043e\u043b\u0436\u043d\u043e \u0431\u044b\u0442\u044c \u043d\u0438 \u043c\u0430\u043b\u0435\u0439\u0448\u0435\u0433\u043e \u043e\u043f\u0430\u0441\u0435\u043d\u0438\u044f, \u043d\u0430\u043f\u0440\u044f\u0436\u0435\u043d\u0438\u044f, \u0441\u0442\u0440\u0435\u0441\u0441\u0430 \u0438\u043b\u0438 \u0441\u0440\u043e\u0447\u043d\u043e\u0441\u0442\u0438.<\/span><\/p>\n<p><span style=\"font-family: arial, helvetica, sans-serif; font-size: 20px;\">\u0414\u043b\u044f \u0434\u043e\u0441\u0442\u0438\u0436\u0435\u043d\u0438\u044f \u043a\u043e\u043d\u0435\u0447\u043d\u043e\u0439 \u0446\u0435\u043b\u0438 \u00ab\u0432\u044b\u0441\u0448\u0435\u0439 \u043d\u0435\u043f\u0440\u0435\u0432\u043e\u0441\u0445\u043e\u0434\u0438\u043c\u043e\u0439 \u043f\u0443\u0441\u0442\u043e\u0442\u044b\u00bb \u0443\u043c \u0434\u043e\u043b\u0436\u0435\u043d \u0431\u044b\u0442\u044c \u0441\u0432\u043e\u0431\u043e\u0434\u043d\u044b\u043c \u0438 \u043d\u0435\u043f\u0440\u0438\u0432\u044f\u0437\u0430\u043d\u043d\u044b\u043c, \u0431\u0435\u0437 \u043f\u0440\u0438\u0437\u043d\u0430\u043a\u0430, \u0431\u0435\u0437 \u0441\u0438\u043c\u0432\u043e\u043b\u0430. \u0422\u043e \u0435\u0441\u0442\u044c \u043d\u0435 \u0434\u043e\u043b\u0436\u043d\u043e \u0431\u044b\u0442\u044c \u043d\u0438 \u043c\u0430\u043b\u0435\u0439\u0448\u0435\u0433\u043e \u043c\u0435\u0440\u0446\u0430\u043d\u0438\u044f \u043c\u044b\u0441\u043b\u0435\u0439 \u043d\u0430 \u043f\u0440\u043e\u0442\u044f\u0436\u0435\u043d\u0438\u0438 \u0432\u0441\u0435\u0433\u043e \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0430 \u043d\u0438\u0441\u0445\u043e\u0436\u0434\u0435\u043d\u0438\u044f \u0432 \u043f\u0443\u0441\u0442\u043e\u0442\u0443. \u041f\u0435\u0440\u0435\u0434 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0435\u0439 \u0441\u0442\u0430\u0434\u0438\u0435\u0439 \u044d\u0442\u0430 \u043f\u0443\u0441\u0442\u043e\u0442\u0430 \u043c\u043e\u0436\u0435\u0442 \u0432\u0435\u0440\u043d\u0443\u0442\u044c\u0441\u044f \u043a \u043f\u0440\u0435\u0436\u043d\u0438\u043c \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u043d\u044b\u043c \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u044f\u043c, \u0432 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0432\u0441\u0451 \u0435\u0449\u0451 \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u044f\u0442 \u043c\u043d\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0435, \u043d\u0435\u0440\u0435\u0433\u0443\u043b\u044f\u0440\u043d\u044b\u0435 \u0438\u0437\u0432\u0435\u0440\u0436\u0435\u043d\u0438\u044f \u043c\u044b\u0441\u043b\u0435\u0439. \u042d\u0442\u043e \u043e\u0437\u043d\u0430\u0447\u0430\u0435\u0442, \u0447\u0442\u043e \u043f\u0440\u043e\u0446\u0435\u0441\u0441 \u0432\u0441\u0451 \u0435\u0449\u0451 \u043d\u0435\u0441\u0442\u0430\u0431\u0438\u043b\u0435\u043d, \u0438 \u044d\u0442\u043e\u043c\u0443 \u043f\u0440\u043e\u0441\u043a\u0430\u043b\u044c\u0437\u044b\u0432\u0430\u043d\u0438\u044e \u043d\u0443\u0436\u043d\u043e \u043f\u0440\u043e\u0442\u0438\u0432\u043e\u0441\u0442\u043e\u044f\u0442\u044c.<\/span><\/p>\n<p><span style=\"font-family: arial, helvetica, sans-serif; font-size: 20px;\">\u041e\u0434\u043d\u0430\u043a\u043e, \u0438\u0441\u043f\u044b\u0442\u0430\u0432 \u044d\u0442\u043e \u043c\u043d\u043e\u0433\u043e \u0442\u044b\u0441\u044f\u0447 \u0440\u0430\u0437, \u043c\u043e\u0436\u043d\u043e \u0441 \u0431\u043e\u043b\u044c\u0448\u043e\u0439 \u0443\u0434\u0430\u0447\u0435\u0439 \u0438 \u0445\u043e\u0440\u043e\u0448\u0435\u0439 \u043a\u0430\u043c\u043c\u043e\u0439, \u043d\u0430\u043a\u043e\u043d\u0435\u0446, \u043e\u0434\u043d\u0430\u0436\u0434\u044b \u043f\u0440\u0438\u0439\u0442\u0438 \u043a \u044d\u0442\u043e\u043c\u0443 \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u044e \u0431\u0435\u0437\u0437\u043d\u0430\u043a\u043e\u0432\u043e\u0433\u043e (appa\u1e47ihita) \u0431\u0435\u0441\u043f\u0440\u0438\u0437\u043d\u0430\u043a\u043e\u0432\u043e\u0433\u043e (animitta) \u0441\u0438\u044f\u044e\u0449\u0435\u0433\u043e \u0443\u043c\u0430, \u0432 \u043a\u043e\u0442\u043e\u0440\u043e\u043c \u043d\u0435\u0442 \u043d\u0438 \u043c\u0430\u043b\u0435\u0439\u0448\u0435\u0433\u043e \u043f\u043e\u043d\u044f\u0442\u0438\u044f \u00ab\u044f\u00bb \u0438\u043b\u0438 \u00ab\u043c\u043e\u0451\u00bb. \u042d\u0442\u043e \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u0435 \u043c\u043e\u0436\u0435\u0442 \u0431\u044b\u0442\u044c \u0434\u043e\u0441\u0442\u0438\u0433\u043d\u0443\u0442\u043e \u0442\u043e\u043b\u044c\u043a\u043e \u0442\u043e\u0433\u0434\u0430, \u043a\u043e\u0433\u0434\u0430 \u043f\u043e\u0434\u0441\u043e\u0437\u043d\u0430\u043d\u0438\u0435 \u0443\u0436\u0435 \u043f\u043e\u043b\u043d\u043e\u0441\u0442\u044c\u044e \u0443\u043d\u0438\u0447\u0442\u043e\u0436\u0438\u043b\u043e \u0436\u0430\u0434\u043d\u043e\u0441\u0442\u044c, \u043d\u0435\u043d\u0430\u0432\u0438\u0441\u0442\u044c \u0438 \u0437\u0430\u0431\u043b\u0443\u0436\u0434\u0435\u043d\u0438\u0435. \u042d\u0442\u043e \u043e\u0437\u043d\u0430\u0447\u0430\u0435\u0442, \u0447\u0442\u043e \u043f\u044f\u0442\u044c \u043e\u043a\u043e\u0432, \u043e\u0442\u0431\u0440\u0430\u0441\u044b\u0432\u0430\u0435\u043c\u044b\u0445 \u043d\u0435\u0432\u043e\u0437\u0432\u0440\u0430\u0449\u0430\u044e\u0449\u0438\u043c\u0438\u0441\u044f (\u0430\u043d\u0430\u0433\u0430\u043c\u0438\u043d\u044b) \u0431\u044b\u043b\u0438 \u0443\u043d\u0438\u0447\u0442\u043e\u0436\u0435\u043d\u044b (\u0441\u043c. \u0441\u0442\u0430\u0442\u044c\u044e \u043e \u0422\u0445\u0435\u0440\u0430\u0432\u0430\u0434\u0435, \u0434\u0440\u0435\u0432\u043d\u0435\u0439\u0448\u0435\u0439 \u0444\u043e\u0440\u043c\u0435 \u0431\u0443\u0434\u0434\u0438\u0437\u043c\u0430).<\/span><\/p>\n<p><span style=\"font-family: arial, helvetica, sans-serif; font-size: 20px;\">\u0412 \u044d\u0442\u043e\u043c \u043f\u043e\u0433\u0440\u0443\u0436\u0435\u043d\u0438\u0438 \u043d\u0430 \u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u0435 \u0441\u0442\u0430\u0434\u0438\u0438 \u043f\u0443\u0441\u0442\u043e\u0442\u043d\u043e\u0441\u0442\u0438 \u0412\u0430\u0448 \u043f\u0443\u0442\u044c \u0441 \u0435\u0434\u0438\u043d\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c \u043f\u0443\u043d\u043a\u0442\u043e\u043c \u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u0438\u044f \u043c\u043e\u0436\u0435\u0442 \u043d\u0430\u0447\u0438\u043d\u0430\u0442\u044c\u0441\u044f \u0441 \u043e\u0433\u0440\u043e\u043c\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u0441\u0442\u0432\u0430 \u043f\u0440\u0438\u0440\u043e\u0434\u044b, \u0442\u0430\u043a\u043e\u0433\u043e \u043a\u0430\u043a \u043b\u0435\u0441, \u043c\u043e\u0440\u0435 \u0438\u043b\u0438 \u043d\u0435\u0431\u043e. \u0412 \u044d\u0442\u0438\u0445 \u0441\u0446\u0435\u043d\u0430\u0440\u0438\u044f\u0445 \u043d\u0435 \u0434\u043e\u043b\u0436\u043d\u043e \u0431\u044b\u0442\u044c \u043d\u0438\u043a\u0430\u043a\u0438\u0445 \u0436\u0438\u0432\u044b\u0445 \u0441\u0443\u0449\u0435\u0441\u0442\u0432. \u042d\u0442\u043e \u043d\u0435 \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0430\u043b\u044c\u043d\u044b\u0439 \u0438\u043b\u0438 \u043d\u0435\u0438\u0437\u0431\u0435\u0436\u043d\u044b\u0439 \u0441\u0446\u0435\u043d\u0430\u0440\u0438\u0439. \u0422\u043e\u0433\u0434\u0430 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0439 \u0431\u043e\u043b\u0435\u0435 \u0433\u043b\u0443\u0431\u043e\u043a\u0438\u0439 \u0441\u043b\u043e\u0439 \u043c\u043e\u0436\u043d\u043e \u0440\u0430\u0441\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u0442\u044c \u0442\u043e\u043b\u044c\u043a\u043e \u043a\u0430\u043a \u0437\u0435\u043c\u043b\u044e \u0438\u043b\u0438 \u043c\u043e\u0440\u0435 (\u044d\u0442\u043e \u0435\u0441\u0442\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0441\u043f\u043e\u0441\u043e\u0431 \u043f\u043e\u0433\u0440\u0443\u0436\u0435\u043d\u0438\u044f \u0432 \u0433\u043b\u0443\u0431\u0438\u043d\u044b). \u042d\u0442\u043e\u0442 \u0441\u043b\u043e\u0439 \u043c\u043e\u0436\u0435\u0442 \u0432\u043e\u0437\u043d\u0438\u043a\u043d\u0443\u0442\u044c \u0438\u043b\u0438 \u043d\u0435 \u0432\u043e\u0437\u043d\u0438\u043a\u043d\u0443\u0442\u044c: \u043e\u043d \u043d\u0435 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0435\u0441\u0442\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c \u043f\u0440\u043e\u0434\u043e\u043b\u0436\u0435\u043d\u0438\u0435\u043c \u0432\u044b\u0448\u0435\u043f\u0435\u0440\u0435\u0447\u0438\u0441\u043b\u0435\u043d\u043d\u043e\u0433\u043e. \u041e\u0434\u043d\u0430\u043a\u043e \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0439 \u0431\u043e\u043b\u0435\u0435 \u0433\u043b\u0443\u0431\u043e\u043a\u0438\u0439 \u0441\u043b\u043e\u0439 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0431\u043e\u043b\u0435\u0435 \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0430\u043b\u044c\u043d\u044b\u043c, \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u044b\u043c: \u0431\u0435\u0441\u043a\u043e\u043d\u0435\u0447\u043d\u043e\u0435 \u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u0441\u0442\u0432\u043e.<\/span><\/p>\n<p><span sty