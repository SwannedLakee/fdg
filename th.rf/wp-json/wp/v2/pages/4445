\n\u0422\u0430\u043a\u043e\u0432 \u043f\u0435\u0440\u0432\u044b\u0439 \u0448\u0430\u0433, \u043a\u043e\u0433\u0434\u0430 \u043c\u044b \u0434\u043e\u0441\u0442\u0438\u0433\u043b\u0438 \u0441\u0442\u0430\u0431\u0438\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u0443\u043c\u0430, \u043a\u043e\u0433\u0434\u0430 \u043c\u044b \u043d\u0430\u0441\u0442\u043e\u0440\u043e\u0436\u0435, \u043e\u0442\u043a\u0440\u044b\u0432\u0430\u0435\u043c\u0441\u044f \u0438 \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u0435\u043c \u0447\u0435\u043c\u0443-\u0443\u0433\u043e\u0434\u043d\u043e \u0432\u043e\u0437\u043d\u0438\u043a\u043d\u0443\u0442\u044c \u0432 \u043d\u0435\u043c \u2014 \u044d\u0442\u043e \u043e\u0441\u043e\u0437\u043d\u0430\u0432\u0430\u043d\u0438\u0435 \u0431\u0435\u0437 \u0432\u044b\u0431\u043e\u0440\u0430. \u0418\u043c\u0435\u043d\u043d\u043e \u0442\u0430\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c \u043c\u044b \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u0435\u043c \u0432\u0435\u0449\u0430\u043c \u0438\u0437 \u0431\u0435\u0441\u0441\u043e\u0437\u043d\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u043f\u043e\u0434\u043d\u044f\u0442\u044c\u0441\u044f \u043d\u0430 \u043f\u043e\u0432\u0435\u0440\u0445\u043d\u043e\u0441\u0442\u044c \u043d\u0430\u0448\u0438\u0445 \u0443\u043c\u043e\u0432. \u0412 \u044d\u0442\u043e\u0439 \u043c\u0435\u0434\u0438\u0442\u0430\u0446\u0438\u0438 \u0443 \u043d\u0430\u0441 \u043d\u0435\u0442 \u043e\u0431\u044a\u0435\u043a\u0442\u0430, \u043c\u044b \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u0435\u043c \u043b\u044e\u0431\u043e\u0439 \u043c\u044b\u0441\u043b\u0438, \u044d\u043c\u043e\u0446\u0438\u0438 \u0438\u043b\u0438 \u043e\u0449\u0443\u0449\u0435\u043d\u0438\u044e \u0432\u043e\u0437\u043d\u0438\u043a\u043d\u0443\u0442\u044c. \u041c\u044b \u043d\u0435 \u0431\u043e\u0438\u043c\u0441\u044f. \u041c\u044b \u043d\u0435 \u0432\u044b\u043d\u043e\u0441\u0438\u043c \u0441\u0443\u0436\u0434\u0435\u043d\u0438\u0439.<br \/>\n\u0412 \u043c\u0435