43e\u0440\u044b\u0435 \u043e\u0442\u0448\u0435\u043b\u044c\u043d\u0438\u043a\u0438 \u0438 \u0431\u0440\u0430\u0445\u043c\u0430\u043d\u044b, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043f\u0440\u0438\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u044e\u0442\u0441\u044f \u0442\u0430\u043a\u043e\u0433\u043e \u0432\u0437\u0433\u043b\u044f\u0434\u0430:<\/span><span style=\"color: #212121;\"> \u00ab\u0423\u0434\u043e\u0432\u043e\u043b\u044c\u0441\u0442\u0432\u0438\u0435 \u0438 \u0431\u043e\u043b\u044c, \u044f \u0441\u0430\u043c \u0438 \u043c\u0438\u0440, \u043d\u0438 \u0432\u0435\u0447\u043d\u044b, \u043d\u0438 \u043d\u0435 \u0432\u0435\u0447\u043d\u044b. \u0422\u043e\u043b\u044c\u043a\u043e \u044d\u0442\u043e \u0438\u0441\u0442\u0438\u043d\u0430,\u00a0\u0432\u0441\u0435\u00a0\u043e\u0441\u0442\u0430\u043b\u044c\u043d\u043e\u0435 \u2013 \u043b\u043e\u0436\u043d\u043e!\u00bb.<\/span><\/span><\/p>\n<p><span style=\"font-size: 20px; font-family: arial, helvetica, sans-serif;\"><span style=\"color: #212121;\">\u0415\u0441\u0442\u044c \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043e\u0442\u0448\u0435\u043b\u044c\u043d\u0438\u043a\u0438 \u0438 \u0431\u0440\u0430\u0445\u043c\u0430\u043d\u044b, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043f\u0440\u0438\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u044e\u0442\u0441\u044f \u0442\u0430\u043a\u043e\u0433\u043e \u0432\u0437\u0433\u043b\u044f\u0434\u0430: <\/span><span style=\"color: #212121;\">\u00ab\u0423\u0434\u043e\u0432\u043e\u043b\u044c\u0441\u0442\u0432\u0438\u0435 \u0438 \u0431\u043e\u043b\u044c, \u044f \u0441\u0430\u043c \u0438 \u043c\u0438\u0440 \u0441\u043e\u0437\u0434\u0430\u043d\u044b \u0441\u0430\u043c\u0438\u043c \u0441\u043e\u0431\u043e\u0439. \u0422\u043e\u043b\u044c\u043a\u043e \u044d\u0442\u043e \u0438\u0441\u0442\u0438\u043d\u0430,\u00a0\u0432\u0441\u0435\u00a0\u043e\u0441\u0442\u0430\u043b\u044c\u043d\u043e\u0435 \u2013 \u043b\u043e\u0436\u043d\u043e!\u00bb.<\/span><\/span><\/p>\n<p><span style=\"font-s