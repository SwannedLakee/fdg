43d\u0435 \u0441\u043c\u043e\u0436\u0435\u0442 \u0432\u0430\u0441 \u0440\u0430\u0441\u043f\u0430\u043b\u0438\u0442\u044c. \u0423 \u043a\u043e\u0433\u043e \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u043c\u0443\u0434\u0440\u043e\u0441\u0442\u0438, \u0442\u043e\u0442 \u043d\u0438\u043a\u043e\u0433\u0434\u0430 \u043d\u0435 \u0441\u0442\u0430\u043d\u0435\u0442 \u0440\u0430\u0431\u043e\u043c \u0436\u0435\u043b\u0430\u043d\u0438\u044f\u00bb.<\/span><\/p>\n<p><span style=\"color: #1d2129; font-size: 14pt; font-family: arial, helvetica, sans-serif;\">14. \u0412\u043e\u043f\u0440\u043e\u0441: \u0427\u0442\u043e \u0432 \u043c\u0438\u0440\u0435 \u0441\u0430\u043c\u043e\u0435 \u0445\u0443\u0434\u0448\u0435\u0435?<\/span><\/p>\n<p><span style=\"color: #1d2129; font-size: 14pt; font-family: arial, helvetica, sans-serif;\">\u041e\u0442\u0432\u0435\u0442: \u041d\u0435\u0432\u0435\u0434\u0435\u043d\u0438\u0435 \u2013 \u0432\u043e\u0442, \u0447\u0442\u043e \u0441\u0430\u043c\u043e\u0435 \u0445\u0443\u0434\u0448\u0435\u0435 \u0432 \u043c\u0438\u0440\u0435.<\/span><\/p>\n<p><span style=\"color: #1d2129; font-size: 14pt; font-family: arial, helvetica, sans-serif;\">\u041f\u043e\u0447\u0435\u043c\u0443?<br \/>\n\u0430) \u041f\u043e\u0442\u043e\u043c\u0443 \u0447\u0442\u043e \u043d\u0435\u0432\u0435\u0434\u0435\u043d\u0438\u0435 \u0441\u043e\u0437\u0434\u0430\u0451\u0442 \u043c\u0438\u0440.<br \/>\n\u0431) \u0416\u0430\u0436\u0434\u0430, \u043d\u0435\u043d\u0430\u0432\u0438\u0441\u0442\u044c, \u0437\u0430\u0431\u043b\u0443\u0436\u0434\u0435\u043d\u0438\u0435, \u0441\u0442\u0440\u0430\u0434\u0430\u043d\u0438\u044f \u0438 \u043c\u043d\u043e\u0433\u043e \u0435\u0449\u0451 \u0434\u0440\u0443\u0433\u043e\u0433\u043e \u043f\u043b\u043e\u0445\u043e\u0433\u043e \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u0438\u0442 \u043e\u0442 \u043d\u0435\u0432\u0435\u0434\u0435\u043d\u0438\u044f. \u041d\u0435\u0432\u0435\u0434\u0435\u043d\u0438\u0435 \u2013 \u044d\u0442\u043e \u043c\u04