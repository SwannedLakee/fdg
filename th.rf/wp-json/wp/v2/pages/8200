43c\u0435\u0441\u0442\u0435 \u0441 \u0431\u043b\u0430\u0436\u0435\u043d\u0441\u0442\u0432\u043e\u043c; \u0441\u043e\u0441\u0440\u0435\u0434\u043e\u0442\u043e\u0447\u0435\u043d\u0438\u0435, \u043a\u043e\u0442\u043e\u0440\u043e\u0435 \u043f\u043e\u0440\u043e\u0436\u0434\u0430\u0435\u0442\u0441\u044f \u0432\u043c\u0435\u0441\u0442\u0435 \u0441 \u0431\u0435\u0441\u0441\u0442\u0440\u0430\u0441\u0442\u0438\u0435\u043c. \u041f\u0435\u0440\u0432\u043e\u0435 \u0438 \u0432\u0442\u043e\u0440\u043e\u0435 \u043c\u0435\u0434\u0438\u0442\u0430\u0442\u0438\u0432\u043d\u043e\u0435 \u043f\u043e\u0433\u043b\u043e\u0449\u0435\u043d\u0438\u0435 (\u0434\u0436\u0445\u0430\u043d\u044b) &#171;\u043f\u043e\u0440\u043e\u0436\u0434\u0430\u044e\u0442\u0441\u044f \u0432\u043c\u0435\u0441\u0442\u0435 \u0441 \u0440\u0430\u0434\u043e\u0441\u0442\u044c\u044e&#187;, \u0442\u0440\u0435\u0442\u044c\u044f &#171;\u043f\u043e\u0440\u043e\u0436\u0434\u0430\u0435\u0442\u0441\u044f \u0432\u043c\u0435\u0441\u0442\u0435 \u0441 \u0431\u043b\u0430\u0436\u0435\u043d\u0441\u0442\u0432\u043e\u043c&#187; \u0438 \u0447\u0435\u0442\u0432\u0435\u0440\u0442\u043e\u0435 \u043c\u0435\u0434\u0438\u0442\u0430\u0442\u0438\u0432\u043d\u043e\u0435 \u043f\u043e\u0433\u043b\u043e\u0449\u0435\u043d\u0438\u0435 (\u0434\u0436\u0445\u0430\u043d\u0430) &#171;\u043f\u043e\u0440\u043e\u0436\u0434\u0430\u0435\u0442\u0441\u044f \u0432\u043c\u0435\u0441\u0442\u0435 \u0441 \u043d\u0435\u0432\u043e\u0437\u043c\u0443\u0442\u0438\u043c\u043e\u0441\u0442\u044c\u044e&#187;. <\/span><\/span><\/p>\n<p align=\"LEFT\"><span style=\"color: #000000; font-size: 20px;\"> <span style=\"font-family: Arial, sans-serif;\">\u0418 \u043e\u043f\u044f\u0442\u044c \u0436\u0435, \u0435\u0441\u0442\u044c \u0442\u0440\u0438 \u0432\u0438\u0434\u0430 \u0441\u043e\u0441\u0440\u0435\u0434\u043e\u0442\u043e\u0447\u0435\u043d\u0438\u044f: \u0443\u043c\u0435\u043b\u043e\u0435 \u0441\u043e\u0441\u0440\u0435\u0434\u043e\u0442\u043e\u0447\u0435\u043d\u0438\u0435; \u0443\u04