43e\u0439 \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u0438 \u0442\u0430\u043a\u043e\u0432\u0430, \u0447\u0442\u043e \u0438\u043d\u043e\u0433\u0434\u0430 \u0441\u0435\u0440\u0434\u0446\u0435 \u0432\u0445\u043e\u0434\u0438\u0442 \u0432 <em>\u0443\u043f\u0430\u0447\u0430\u0440\u0430<\/em>, \u043d\u043e \u0432 \u0434\u0440\u0443\u0433\u043e\u0439 \u0440\u0430\u0437 \u043c\u043e\u0436\u0435\u0442 \u0432\u043e\u0439\u0442\u0438 \u0442\u043e\u043b\u044c\u043a\u043e \u0432 <em>\u043a\u0445\u0430\u043d\u0438\u043a\u0430 \u0441\u0430\u043c\u0430\u0434\u0445\u0438<\/em>. \u041e\u0434\u043d\u0430\u043a\u043e \u0444\u0443\u043d\u0434\u0430\u043c\u0435\u043d\u0442 <em>\u0443\u043f\u0430\u0447\u0430\u0440\u0430 \u0441\u0430\u043c\u0430\u0434\u0445\u0438<\/em> \u0438 \u0443\u0436\u0435 \u043e\u0431\u0440\u0435\u0442\u0435\u043d\u043d\u043e\u0435 \u043f\u0440\u043e\u0437\u0440\u0435\u043d\u0438\u0435 &#8212; \u044d\u0442\u043e \u0440\u0435\u0430\u043b\u044c\u043d\u0430\u044f \u0441\u0438\u043b\u0430 \u0434\u0432\u0438\u0436\u0443\u0449\u0430\u044f \u0443\u043c\u043e\u043c, \u0442\u043e\u0447\u043d\u043e \u0442\u0430\u043a \u0436\u0435, \u043a\u0430\u043a \u043f\u0435\u0440\u0432\u0430\u044f \u0441\u0442\u0443\u043f\u0435\u043d\u044c \u0440\u0430\u043a\u0435\u0442\u044b, \u043a\u043e\u0442\u043e\u0440\u0430\u044f, \u0445\u043e\u0442\u044f \u0441\u0435\u0439\u0447\u0430\u0441 \u0438 \u043d\u0435 \u0438\u043c\u0435\u0435\u0442 \u044d\u043d\u0435\u0440\u0433\u0438\u0438, \u043d\u043e \u0443\u0436\u0435 \u043f\u043e\u043c\u043e\u0433\u043b\u0430 \u043d\u0430\u043c \u0434\u043e\u0441\u0442\u0438\u0447\u044c \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u043d\u043e\u0439 \u0432\u044b\u0441\u043e\u0442\u044b. \u0412\u0442\u043e\u0440\u0430\u044f \u0441\u0442\u0443\u043f\u0435\u043d\u044c \u0440\u0430\u043a\u0435\u0442\u044b \u043c\u043e\u0436\u0435\u0442 \u0442\u0435\u043f\u0435\u0440\u044c \u043f\u043e\u0434\u043d\u0438\u043c\u0430\u0442\u044c \u043d\u0430\u0441 \u0432\u0441\u0435 \u0432\u044b\u0448\u0435 \u0438 \u0432\u044b\u0448\u0435.<\/span><\/p>\n<p><span style=\"font-family: arial, helvetica, sans-serif; font-size: 20px;\">\u041a\u0430\u043a\u0438\u043c \u0431\u044b \u043d\u0438 \u0431\u044b\u043b \u043d\u0430\u0448 \u043c\u0435\u0442\u043e\u0434 \u043c\u0435\u0434\u0438\u0442\u0430\u0446\u0438\u0438, \u0431\u0443\u0434\u044c \u0442\u043e \u0441\u043e\u0437\u0435\u0440\u0446\u0430\u043d\u0438\u0435 \u0442\u0435\u043b\u0430, \u043e\u0441\u043e\u0437\u043d\u0430\u043d\u043d\u043e\u0441\u0442\u044c \u0434\u044b\u0445\u0430\u043d\u0438\u044f \u0438\u043b\u0438 \u043e\u0441\u043e\u0437\u043d\u0430\u0432\u0430\u043d\u0438\u0435 \u0441\u043c\u0435\u0440\u0442\u0438, \u0432\u044b \u0434\u043e\u043b\u0436\u043d\u044b \u0447\u0430\u0441\u0442\u043e \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u043e\u0432\u0430\u0442\u044c \u0438 \u0440\u0430\u0437\u0432\u0438\u0432\u0430\u0442\u044c \u0435\u0433\u043e. \u041d\u0430 \u044d\u0442\u043e\u043c \u044d\u0442\u0430\u043f\u0435 \u043d\u0430\u0448\u0435\u0439 \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u0438 \u0441\u0435\u0440\u0434\u0446\u0435 \u0441\u0442\u0430\u043d\u043e\u0432\u0438\u0442\u0441\u044f \u0432\u0441\u0435 \u0431\u043e\u043b\u0435\u0435 \u0441\u043f\u043e\u043a\u043e\u0439\u043d\u044b\u043c \u0438 \u0431\u0435\u0437\u043c\u044f\u0442\u0435\u0436\u043d\u044b\u043c. \u042d\u0442\u043e \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u0435 \u0431\u0435\u0437\u043c\u044f\u0442\u0435\u0436\u043d\u043e\u0441\u0442\u0438 \u043f\u043e\u043b\u043d\u043e\u0441\u0442\u044c\u044e \u043d\u0430\u043f\u043e\u043b\u043d\u044f\u0435\u0442 \u0441\u0435\u0440\u0434\u0446\u0435 \u0438 \u043c\u043e\u0436\u0435\u0442 \u0434\u043b\u0438\u0442\u044c\u0441\u044f \u0434\u043e\u043b\u0433\u0438\u0435 \u0433\u043e\u0434\u044b. \u041f\u0440\u0438 \u0441\u0442\u0430\u0431\u0438\u043b\u044c\u043d\u043e\u0439 \u0432\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u0441\u0435\u0440\u0434\u044